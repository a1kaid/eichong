package com.bluemobi.model.template;

import com.bluemobi.model.GameObject;

public class BulletTemplate extends GameObject {
	//子弹编号
	private int id;
	//射击类型
	private int type;
	//子弹大小
	private int size;
	//爆炸特效
	private String explodeEffect;
	//碰撞偏移
	private float offset;
	//碰撞效果
	private int effect;
	//爆炸范围
	private int range;
	//碰撞动画
	private String animation;
	//飞行速度
	private int speed;
	//子弹飞行动画
	private String flyingAnimation;
	//子弹拖尾动画2
	private String tailAni;
	//击中音效
	private String sound;
	/**获取子弹编号*/
	public int getId() {
		return this.id;
	}
	/**设置子弹编号*/
	public void setId(int id) {
		this.id = id;
	}
	/**获取射击类型*/
	public int getType() {
		return this.type;
	}
	/**设置射击类型*/
	public void setType(int type) {
		this.type = type;
	}
	/**获取子弹大小*/
	public int getSize() {
		return this.size;
	}
	/**设置子弹大小*/
	public void setSize(int size) {
		this.size = size;
	}
	/**获取爆炸特效*/
	public String getExplodeEffect() {
		return this.explodeEffect;
	}
	/**设置爆炸特效*/
	public void setExplodeEffect(String explodeEffect) {
		this.explodeEffect = explodeEffect;
	}
	/**获取碰撞偏移*/
	public float getOffset() {
		return this.offset;
	}
	/**设置碰撞偏移*/
	public void setOffset(float offset) {
		this.offset = offset;
	}
	/**获取碰撞效果*/
	public int getEffect() {
		return this.effect;
	}
	/**设置碰撞效果*/
	public void setEffect(int effect) {
		this.effect = effect;
	}
	/**获取爆炸范围*/
	public int getRange() {
		return this.range;
	}
	/**设置爆炸范围*/
	public void setRange(int range) {
		this.range = range;
	}
	/**获取碰撞动画*/
	public String getAnimation() {
		return this.animation;
	}
	/**设置碰撞动画*/
	public void setAnimation(String animation) {
		this.animation = animation;
	}
	/**获取飞行速度*/
	public int getSpeed() {
		return this.speed;
	}
	/**设置飞行速度*/
	public void setSpeed(int speed) {
		this.speed = speed;
	}
	/**获取子弹飞行动画*/
	public String getFlyingAnimation() {
		return this.flyingAnimation;
	}
	/**设置子弹飞行动画*/
	public void setFlyingAnimation(String flyingAnimation) {
		this.flyingAnimation = flyingAnimation;
	}
	/**获取子弹拖尾动画2*/
	public String getTailAni() {
		return this.tailAni;
	}
	/**设置子弹拖尾动画2*/
	public void setTailAni(String tailAni) {
		this.tailAni = tailAni;
	}
	/**获取击中音效*/
	public String getSound() {
		return this.sound;
	}
	/**设置击中音效*/
	public void setSound(String sound) {
		this.sound = sound;
	}
}
