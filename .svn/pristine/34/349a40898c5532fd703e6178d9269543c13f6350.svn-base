@charset "utf-8";
body,html{
    font-family: '微软雅黑';
    background-color: #F3F3F4;;
}
.userBaseInfo{
    background-color: #ffffff;
    border-top: 4px solid #E5E5E5;
    margin-top: 10px;
    padding:14px 14px 20px 14px;
}
.clearfix{
    　　zoom:1;
}

.clearfix:after{
    content:"";
    display:block;
    clear:both;
}
.ibox-content {
    background-color: #F3F3F4 !important;
    color: inherit;
    /* padding: 15px 20px 20px; */
    border-color: #e7eaec;
    -webkit-border-image: none;
    -o-border-image: none;
    border-image: none;
}
.goBack{
    display: inline-block;
    width: 80px;
    height: 30px;
    line-height: 30px;
    color: #ffffff;
    border-radius: 4px;
    text-align: center;
    background-color: #bfbfbf;
    margin-right: 20px;

}
.cursor{
    cursor: pointer;
}
.editBtn{
    display: inline-block;
    width: 80px;
    height: 30px;
    line-height: 30px;
    color: #ffffff;
    background-color: #ff7d00;
    text-align: center;
    border-radius:4px;
    font-size: 13px;
    margin-right: 20px;
}
.saveBtn{
    display: inline-block;
    width: 80px;
    height: 30px;
    line-height: 30px;
    color: #ffffff;
    background-color: #ff7d00;
    text-align: center;
    border-radius:4px;
    font-size: 13px;
    margin-right: 20px;
}
.btnBlock{
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    margin-top: 5px;
}
/*.titleTip{*/
    /*display: flex;*/
    /*flex-direction: row;*/
    /*justify-content: space-between;*/
/*}*/
.titleTip span{
    font-size: 15px;
    font-weight: 900;
    line-height: 38px;
}
.baseInfoLeft{
    border: 1px solid #E7EAEC;
    list-style: none;
    padding: 0;
}
.baseInfoRight{
    border: 1px solid #E7EAEC;
    border-left: 0;
    list-style: none;
    padding: 0;
}
.baseInfoLeft li,.baseInfoRight li{
    height: 38px;
    line-height: 38px;
    padding: 0 20px;
    border-bottom: 1px solid #E7EAEC;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.baseInfoLeft li:last-child,.baseInfoRight li:last-child{
    border-bottom: 0;
}
.infoTip{
    font-size: 12px;
    color: #666666;
}
.infoValue{
    font-size: 12px;
    color: #333333;
}
.rateValue{
    color: #ff7d00;
    text-decoration: underline;
}
table thead th{
    text-align: center;
    font-size: 14px;
    font-family: "微软雅黑";
    background-color: #ffffff !important;
}
table tbody tr td{
    text-align: center;
    font-size: 14px;
    font-family: "微软雅黑";
    background-color: #ffffff !important;
}
.modelTitle{
    height:34px;
    line-height: 34px;
    font-size: 12px;
    font-weight: 600;
}
.titleTip .addBtn{
    font-size: 12px;
    display: inline-block;
    width: 60px;
    height: 22px;
    line-height: 22px;
    color: #ffffff;
    background-color: #ff7d00;
    text-align: center;
    border-radius:4px;
    margin-left:40px;
    cursor: pointer;
}
.typeB{
   margin-left: 0;

}
.typeUl{
    padding-left:0;
}
.typeUl li{
    list-style: none;
}
.typeUl li:hover{
    background-color: #ff7d00;
    color:#ffffff;
}
.queryParam{
    padding:0;
    line-height: 32px;
    font-size: 14px;
    text-align: left;
    width:80px;
    float: left;
}
.model-select-box {
    height: 32px;
    line-height: 32px;
    border: 1px solid #E7EAEC;
    float: left;
    text-indent: 5px;
    position: relative;
    width:140px;
}
.downRow {
    width: 32px;
    height: 31px;
    background: url(../../img/downR.png) no-repeat center;
    position: absolute;
    right: 0;
    top: 0;
    border-left: 1px solid #DCDCDC;
    cursor: pointer;
    z-index: 0;
}
.model-select-option {
    display: none;
    position: absolute;
    background: #fff;
    width: 100%;
    left: -1px;
    border: 1px solid #aaa;
    z-index: 5000;
    max-height: 200px;
    overflow-y: auto;
    overflow-x: hidden;
}
.addBomUl li{
    list-style: none;
    height:36px;
    line-height:36px;
}
.addBomUl li span{
     width:80px;
     text-align: left;
     height:26px;
     line-height:26px;
    display: inline-block;
 }
.addBomUl li input{
    text-align: left;
    height:26px;
    line-height:26px;
    font-size: 12px;
}
.baseInfoLeft li input{
    border:0;
    outline:none;
    border-left:1px solid #dcdcdc;
    width:280px;
    text-align: right;
    padding-right: 20px;
}
.baseInfoLeft li input:focus{
    outline:none;
    border:1px solid #ff7d00;
}
.baseInfoRight li input{
    border:0;
    outline:none;
    border-left:1px solid #dcdcdc;
    width:280px;
    text-align: right;
    padding-right: 20px;
}
.baseInfoRight li input:focus{
    outline:none;
    border:1px solid #ff7d00;
}