        <!--按钮部分开始==========-->
        <div class="btnBlock">
            <div class="col-sm-10 col-xs-12 col-md-10" >
                <div class="col-sm-1 col-xs-1  btnStyle" id="addList" style="display: none;">
                    新建
                </div>
                <div class="col-sm-1 col-xs-1 col-sm-offset-1 col-xs-offset-1 btnStyle" id="editList" style="display: none;">
                    编辑
                </div>
                <div class="col-sm-1 col-xs-1 col-sm-offset-1 col-xs-offset-1 btnStyle" id="delList" style="display: none;">
                    删除
                </div>
                <div class="col-sm-1 col-xs-1 col-sm-offset-1 col-xs-offset-1 btnStyle" id="closeList" style="display: none;">
                    下架
                </div>
                <div class="col-sm-1 col-xs-1 col-sm-offset-1 col-xs-offset-1 btnStyle" id="editBtnSort" style="display: none;">
                    排序
                </div>
            </div>
            <div class="col-sm-2 col-md-2"></div>
        </div>
        <!--表格部分开始========-->
        <div class="showSet table-responsive">
            <table class=" table table-bordered text-center" style="border: 1px solid #DCDCDC;">
                <thead>
                <tr>
                    <th class="">选择</th>
                    <th class="appBtnList_num">顺序</th>
                    <th class="appBtnList_picture">图片</th>
                    <th class="appBtnList_name">名称</th>
                    <th class="appBtnList_editDate">配置/编辑时间</th>
                    <th class="appBtnList_status">状态</th>
                    <th class="appBtnList_remark">备注</th>
                </tr>
                </thead>
                <tbody id="appBtnTb">

                </tbody>
            </table>

            <div id="appBtnListPage" class="pagination">
            </div>
        </div>
    </div>
</div>
<div class="pictureShow" style="display: none;">
    <img src="" alt="" class="pic" />
</div>
