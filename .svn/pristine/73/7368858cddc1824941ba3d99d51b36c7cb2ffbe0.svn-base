@charset "utf-8";
*{
    margin: 0;
    padding: 0;
}
body {
    background: #F3F3F4;
    font-family: "微软雅黑";
    font-size: 12px;
}
.add_userBlock{
    min-height: 40px;
    background-color: #ffffff;
    border-top: 4px solid #E7EAEC;
    padding: 20px 20px 60px 20px;
    margin-top: 20px;
    position: relative;

}
.add_userBlock .add_greyBlock{
    width: 100%;
    min-height: 80px;
    background-color: #eeeeee;
    padding: 14px 0;
}
.comTitle{
    font-size: 14px;
    font-weight: 600;
    height: 34px;
    line-height: 34px;
}
/*下拉框样式===========*/

ul {
    list-style: none;
}

p {
    margin-bottom: 20px;
}

.model-select-box {
    /*height: 32px;*/
    line-height: 32px;
    border: 1px solid #E7EAEC;
    float: left;
    text-indent: 5px;
    position: relative;
    background-color: #ffffff;
}

.downRow {
    width: 32px;
    height: 31px;
    background: url(../../img/downR.png) no-repeat center;
    position: absolute;
    right: 0;
    top: 0;
    border-left: 1px solid #DCDCDC;
    cursor: pointer;
    z-index: 0;
}

.model-select-text {
    height: 32px;
    cursor: pointer;
    color: #666666;
    -moz-user-select: none;
    -webkit-user-select: none;
    user-select: none;
    background-color: #ffffff;
}

.model-select-option {
    display: none;
    position: absolute;
    background: #fff;
    width: 100%;
    left: -1px;
    border: 1px solid #aaa;
    z-index: 5000;
    max-height: 200px;
    overflow-y: auto;
    overflow-x: hidden;
}

.model-select-option li {
    min-height: 22px;
    line-height: 22px;
    color: #666666;
    cursor: pointer;
}

.model-select-option li.seleced {
    background: #ff7d00;
    color: #fff;
}
/*下拉框结束*/
.comHeight{
    min-height: 32px;
}
.comHeight .commonStyle{
    width: 100%;
    height: 32px;
    outline: none;
    border: 1px solid #DCDCDC;
    background-color: #ffffff;
    text-indent: 20px;
}
.comHeight .commonStyle:focus{
    border: 1px solid #ff7d00;
    text-indent: 20px;
}
.errorStyle{
    color: #FF0000;
    height: 32px;
    line-height: 32px;
}
.commonBtn{
    width: 68px;
    height: 30px;
    border-radius: 4px;
    text-align: center;
    line-height: 30px;
    color: #ffffff;
    display: inline-block;
    position: absolute;
}
.commonBtn:hover{
    cursor: pointer;
}
.saveBtn{
    display: inline-block;
    width: 80px;
    height: 30px;
    line-height: 30px;
    color: #ffffff;
    background-color: #ff7d00;
    text-align: center;
    border-radius:4px;
    font-size: 13px;
    position: absolute;
    right: 120px;
    bottom: 16px;
    cursor: pointer;
}
.quitBtn{
    background-color: #bfbfbf;
    right: 20px;
    bottom: 16px;
    cursor: pointer;
}
.saveBtnStyle{
    background-color: #ff7d00;
}