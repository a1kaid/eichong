@charset "utf-8";
.userBaseInfo{
    background-color: #ffffff;
    border-top: 4px solid #E5E5E5;
    margin-top: 20px;
    padding: 14px;
    background-color: #ffffff;
}
.goBack{
    width: 80px;
    height: 30px;
    line-height: 30px;
    color: #ffffff;
    border-radius: 4px;
    text-align: center;
    background-color: #bfbfbf;
    position: absolute;
    right: 0;
    top: 8px;
}
.cursor{
    cursor: pointer;
}
.baseBlock{
    padding: 10px 10px 10px 0;
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.baseBlock .perblock{
    display: inline-block;
    padding: 6px;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
}
.baseBlock .perblock .titleTip{
    font-size: 14px;
    color: #888888;
    margin-right: 20px;
}
.baseBlock .perblock .parmValue{
    color: #333333;
    font-size: 14px;
    margin-right: 20px;
}
.baseBlock .perblock .tipBtn{
    font-size: 14px;
    color: #ff7d00;
    text-decoration: underline;
}
.baseBlock .perblock .tipBtn:hover{
    cursor: pointer;
}
.adminDateAreaTip{
    padding: 8px;
    font-size: 15px;
    font-weight: 900;
    border-bottom: 1px solid #DDDDDD;

}
.queryTradition{
    padding: 20px 0 15px 8px;
    display: flex;
    flex-direction: row;
}
.queryTradition .titleTip{
    font-size: 14px;
    color: #888888;
    margin-right: 10px;
    line-height: 36px;
}
.inputBlock input{
    outline: none;
    border: 0;
    width: 220px;
    height: 36px;
    border: 1px solid #DDDDDD;
    border-radius: 4px;
}
.queryBtn{
    width: 70px;
    height: 36px;
    background-color: #ff7d00;
    color: #ffffff;
    text-align: center;
    line-height: 36px;
    margin-left: -4px;
    border-radius: 0 4px 4px 0;
}
.queryBtn:hover{
    cursor: pointer;
}
.btnGroup{
    padding: 10px 0 20px 8px;
    display: flex;
    flex-direction: row;
}
