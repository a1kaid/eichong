.btnGroup .btnStyle{
    background: #ff7d00;
    color: #ffffff;
    border-radius: 4px;
    text-align: center;
    height: 28px;
    line-height: 28px;
}
.btnGroup .btnStyle:hover{
    cursor: pointer;
    background: #FF7D00;
    color: #FFFFFF;
}
table thead th{
    text-align: center;
    font-size: 14px;
    font-family: "微软雅黑";
    background-color: #ffffff !important;
}
/*分页样式*/
.pagination {
    width: 100%;
    height: 50px;
    margin: 0;
    padding: 0;
    background: #FFFFFF;
    padding-top:16px ;
}
.pagination ul{
    list-style: none;
}
.pagination ul li{
    float: left;
    margin-right: 10px;
}
.pagination ul li:last-Child{
    list-style: none;
    float: left;
    margin-left: 0;
}
.pagination ul li:nth-last-child(2){
    margin-right: 0;
    /*font-size: 12px;*/
    color: #333333 !important;
    text-decoration: none !important;
}
.pagination ul li:last-Child a{
    pointer-events: none;
    font-size: 12px;
    color: #333333 !important;
    text-decoration: none !important;
    cursor: pointer;
}
.pagination ul li:nth-last-child(2) a{
    pointer-events: none;
    font-size: 12px;
    color: #333333 !important;
    text-decoration: none !important;
    cursor: pointer;
}
.pagination ul li:last-Child a:hover{
    color: #666666;
    text-decoration: none;
}
.pagination ul li:nth-last-child(2) a:hover{
    color: #666666;
    text-decoration: none;
}
.pagination ul .jump{
    margin-top: -4px;
}
.pagination ul li a{
    font-size: 12px;
    color: #333333;
    text-decoration: underline;
    cursor: pointer;
}
.pagination ul li a:hover{
    font-size: 12px;
    color: #FF9900 !important;
    text-decoration: none;
}
.pagination ul li a:active{
    font-size: 12px;
    color: #ff6600 !important;
    text-decoration: none;
}
.newactive a{
    font-size: 12px;
    color: #ff7d00 !important;
    text-decoration: none !important;
}
/*添加电桩样式*/
.ucCardNumber {
    width: 100%;
    border: 1px solid #e1edf7;
    margin-top: 10px;
}
.ucCardNumber input {
    width: 100%;
    height: 28px;
    outline: none;
    border: 0;
    line-height: 28px;
}

/*下拉框样式===========*/

ul {
    list-style: none;
    padding-left: 0;
}

p {
    margin-bottom: 20px;
}

.model-select-box {
    height: 32px;
    line-height: 32px;
    border: 1px solid #E7EAEC;
    float: left;
    text-indent: 5px;
    position: relative;
}

.downRow {
    width: 32px;
    height: 31px;
    background: url(../../img/downR.png) no-repeat center;
    position: absolute;
    right: 0;
    top: 0;
    border-left: 1px solid #DCDCDC;
    cursor: pointer;
    z-index: 0;
}

.model-select-text {
    height: 32px;
    cursor: pointer;
    color: #666666;
    -moz-user-select: none;
    -webkit-user-select: none;
    user-select: none;
}

.model-select-option {
    display: none;
    position: absolute;
    background: #fff;
    width: 100%;
    left: -1px;
    border: 1px solid #aaa;
    z-index: 5000;
    max-height: 170px;
    overflow-y: auto;
    overflow-x: hidden;
}

.model-select-option li {
    min-height: 22px;
    line-height: 22px;
    color: #666666;
    cursor: pointer;
    text-indent: 12px;
}

.model-select-option li.seleced {
    background: #ff7d00;
    color: #fff;
}
/*结束下拉框*/
.provinceDiv{
    width:260px;
    height: 32px;
    margin: 3px 20px;
}
.stationDiv{
   position: relative;
}
.powerStationInput{
    height: 32px;
    line-height: 32px;
    border: 1px solid #DCDCDC;
    width: 100%;
    text-indent: 10px;
    outline: none;
    margin-left: -5px;
}
.powerStationInput:hover{
    border: 1px solid #FF7D00;
}
.powerStationNameUl{
    text-indent: 10px;
    display: none;
    position: absolute;
    top:32px;
    background: #fff;
    width: 100%;
    left: -1px;
    border: 1px solid #aaa;
    z-index: 5000;
    max-height: 106px;
    overflow-y: auto;
    overflow-x: hidden;
}
.powerStationNameUl li{
    min-height:24px;
    line-height: 24px;
    font-size: 12px;
}
.powerStationNameUl li:hover{
    background-color: #ff7d00;
    color:#ffffff;
    cursor: pointer;
}
.stationTip{
    width:260px;
    margin: 0 20px;
    font-size: 12px;
    height: 24px;
    line-height: 24px;
    color:#f00;
}