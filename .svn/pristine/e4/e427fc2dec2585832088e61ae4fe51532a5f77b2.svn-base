package com.wanma.dubbox.model;

import java.util.Date;

import com.wanma.dubbox.model.common.BasicModel;


public class TblUserCard extends BasicModel {
	private static final long serialVersionUID = -2344081692000738049L;
	private String inum;// 内卡号
	private String extNum;// 外卡号
	private Double blc;// 金额
	private Integer comNum;// 公司标识
	private String uid;// 用户ID
	private Integer sts;// 状态 0：正常，1：挂失
	private Integer md; // 卡类型
	
	private String[] comNums;//公司标识数组，查询专用
	private String[] extNums;// 外卡号数组，查询专用
	private String isBd;//是否绑定
	private String qkApply;//是否快速发卡

	public String getInum() {
		return inum;
	}

	public void setInum(String inum) {
		this.inum = inum;
	}

	public String getExtNum() {
		return extNum;
	}

	public void setExtNum(String extNum) {
		this.extNum = extNum;
	}

	public Double getBlc() {
		return blc;
	}

	public void setBlc(Double blc) {
		this.blc = blc;
	}

	public Integer getComNum() {
		return comNum;
	}

	public void setComNum(Integer comNum) {
		this.comNum = comNum;
	}

	public String getUid() {
		return uid;
	}

	public void setUid(String uid) {
		this.uid = uid;
	}

	public Integer getSts() {
		return sts;
	}

	public void setSts(Integer sts) {
		this.sts = sts;
	}

	public Integer getMd() {
		return md;
	}

	public void setMd(Integer md) {
		this.md = md;
	}

	public String[] getComNums() {
		return comNums;
	}

	public void setComNums(String[] comNums) {
		this.comNums = comNums;
	}

	public String[] getExtNums() {
		return extNums;
	}

	public void setExtNums(String[] extNums) {
		this.extNums = extNums;
	}

	public String getIsBd() {
		return isBd;
	}

	public void setIsBd(String isBd) {
		this.isBd = isBd;
	}

	public String getQkApply() {
		return qkApply;
	}

	public void setQkApply(String qkApply) {
		this.qkApply = qkApply;
	}

}
