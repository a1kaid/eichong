@charset "utf-8";
body{
    height:auto!important;
    color: #333;
    font-size: 12px;
    background-color: #f3f3f4;
    font-family: "Microsoft YaHei", "微软雅黑" !important;
}
.userBaseInfo{
    background-color: #ffffff;
    border-top: 4px solid #E5E5E5;
    margin-top: 20px;
    padding: 14px;
    background-color: #ffffff;
}
.queryParam {
    line-height: 25px;
    font-size: 14px;
    text-align: left;
    color: #333;
}
.formBlock {
    width: 100%;
    min-height: 138px;
    height: 100%;
    /*overflow: hidden;*/
    background: #FFFFFF;
    border-top: 3px solid #E5E5E5;
    padding: 26px 18px 20px 20px;
}
.userAccount{
    /*border: 1px solid #DCDCDC;*/
    height: 32px;
}
.userAccount .userAccountInput{
    display: inline;
    width: 100%;
    height: 100%;
    border: 1px solid #DCDCDC;
    outline: none;
    color: #666666;
}
.userAccount .userAccountInput:focus{
    display: inline;
    width: 100%;
    height: 100%;
    border: 1px solid #ff7d00;
    color: #666666;
}
#submitBtn {
    display: inline-block;
    width: 80px;
    height: 30px;
    line-height: 30px;
    color: #ffffff;
    background-color: #ff7d00;
    text-align: center;
    border-radius: 4px;
    font-size: 13px;
    margin: 1px 0 0 5%;
    cursor: pointer;
}