    /*line-height: 32px;*/
    /*display: flex;*/
    /*flex-direction: row;*/
    /*justify-content: space-between;*/
/*}*/
.baseBlock{
    padding: 10px 10px 10px 0;
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.perblock{
    display: inline-block;
    padding: 6px;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.perblock .titleTip{
    font-size: 14px;
    color: #888888;
    margin-right: 20px;
}
.perblock .parmValue{
    color: #333333;
    font-size: 14px;
    margin-right: 20px;
}
.typeDiv{
    padding: 0;
    height:32px;
    line-height: 32px;
}
.sizeBlock:before,.sizeBlock:after {
    content: "";
    display: block;
    clear: both;
}
.sizeBlock {
    zoom: 1;
}
