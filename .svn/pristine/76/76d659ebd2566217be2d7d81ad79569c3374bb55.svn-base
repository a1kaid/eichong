@charset "utf-8";
*{
    margin: 0;
    padding: 0;
}
body {
    background: #F3F3F4;
    font-family: "微软雅黑";
    font-size: 12px;
}
.baseInfoShow{
    padding: 20px 20px 12px 20px;
    border-top: 4px solid #E7EAEC;
    background-color: #ffffff;
    margin:0;
}
.baseInfoTitle{
    height:14px;
    line-height: 14px;
    font-size: 14px;
    color:#000;
    font-weight: 600;
    margin-bottom: 12px;
    padding: 0;
}
.paddingLeft{
    padding-left: 0;
}
.paddingRight{
    padding-right: 0;
}
