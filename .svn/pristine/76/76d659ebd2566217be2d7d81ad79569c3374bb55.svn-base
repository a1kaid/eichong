/*下拉*/
ul {
    list-style: none;
}
.model-select-box {
    float: left;
    text-indent: 5px;
    position: relative;
    border:1px solid #DCDCDC;
}
.downRow {
    width: 20%;
    height: 100%;
    background: url(../../img/downR.png) no-repeat center;
    position: absolute;
    right: 0;
    top: 0;
    border-left: 1px solid #DCDCDC;
    cursor: pointer;
    z-index: 0;
}

.model-select-text {
    height: 100%;
    width: 100%;
    display: flex;
    align-items: center;
    cursor: pointer;
    color: #666666;
    -moz-user-select: none;
    -webkit-user-select: none;
    user-select: none;
}

.model-select-option {
    display: none;
    position: absolute;
    background: #fff;
    width: 100%;
    left: -1px;
    border: 1px solid #aaa;
    z-index: 5000;
    max-height: 200px;
    overflow-y: auto;
    overflow-x: hidden;
}

.model-select-option li {
    height: 32px;
    line-height: 32px;
    color: #666666;
    cursor: pointer;
    padding-left: 15px !important;
}

.model-select-option li.seleced {
    background: #ff7d00;
    color: #fff;
}
.empty{
    height:20px;
    width:100%;
}
.titleText{
    height:32px;
    width:100%;
    line-height: 32px;
    font-size: 14px;
    color:#000;
    font-weight: 600;
}
.cpyInfo{
    background-color: #ffffff;
    position: relative;
    padding:36px 20px 10px 20px;
    /*border-top: 4px solid #E7EAEC;*/
    border:1px solid #DCDCDC;
}
.cpyInfo .editBtn{
    height:24px;
    width: 60px;
    background-color: #ff7d00;
    color:#ffffff;
    line-height: 24px;
    text-align: center;
    position: absolute;
    right:18px;
    top:10px;
    cursor: pointer;
    border-radius: 4px;
}
.cpyInfo .cpyUl{
    margin-bottom: 0;
}
.cpyInfo .cpyUl li{
    display: flex;
    flex-direction: row;
    height:32px;
    line-height: 32px;
    margin-bottom: 10px;
}
.ivFreightAmount{
    margin-right: 60px;
}
.cpyUl li span{
    display: inline-block;
}
.cpyInfo .cpyUl li input{
    outline: none;
    display: inline-block;
    border:1px solid #DCDCDC;
    margin-right:60px;
    background-color: #ffffff;
    padding-left: 15px;
}
.cpyInfo .cpyUl li input:focus{
    outline: none;
    border:1px solid #ff7d00;
}
.cpyInfo .cpyUl .checkInput{
    text-align: right;
    border: none;
    outline: none;
    line-height: normal;
    margin:9px 0 0;
    width:14px;
    line-height:32px;
}
.btnBox{
    position: relative;
    height: 30px;
    margin-bottom: 5px;
    margin-top: 10px;
}
.btnBox .cancelBtn{
    width:80px;
    height:24px;
    background-color: #ff7d00;
    color:#ffffff;
    line-height: 24px;
    text-align: center;
    position: absolute;
    right:110px;
    top:5px;
    cursor: pointer;
    border-radius: 4px;
}
.btnBox .saveBtn{
    width:80px;
    height:24px;
    background-color: #ff7d00;
    color:#ffffff;
    line-height: 24px;
    text-align: center;
    position: absolute;
    right:18px;
    top:5px;
    cursor: pointer;
    border-radius: 4px;
}
.cpyInfo .cpyUl .ivInvoiceNumber{
    outline: none;
    border:1px solid #DCDCDC;
    width:100%;
    margin-right:60px;
    background-color: #ffffff;
    padding-left: 15px;
}
.cpyUl .tit{
    width:150px;
    display: inline-block;
}
.cpyUl .btnLi{
    position: relative;
}
.cpyUl .lookBtn{
    width:80px;
    height:24px;
    background-color: #ff7d00;
    color:#ffffff;
    line-height: 24px;
    text-align: center;
    position: absolute;
    right:18px;
    top:5px;
    cursor: pointer;
    border-radius: 4px;
}
.cpyUl .icStatus{
    margin:0;
}
/*弹出框设计*/
.bindPileBlock{
    padding-top: 10px;
    min-height:280px;
    /*background-color: #FF0000;*/
}
.bindPileBlock .bindPileTable{
    width: 100%;
}
.table > thead > tr > th {
    vertical-align: bottom;
    border-bottom: 1px solid #ddd;
    font-size: 12px;
}
.table > thead > tr > th, .table > tbody > tr > th, .table > tfoot > tr > th, .table > thead > tr > td, .table > tbody > tr > td, .table > tfoot > tr > td {
    font-size: 12px;
}
.bindPileTable tr:nth-child(odd) {
    background: #FFFFFF;
}
.bindPileTable tr:nth-child(even) {
    background: #f5f5f5;
}
.bindPileTable tbody tr td{
    text-align: center;
}
.bindPileTable thead tr th{
    text-align: center;
}
/*分页*/

/*分页部分开始*/
.pagination ul li{
    float: left;
    margin-right: 10px;
}
.pagination ul li:last-Child{
    list-style: none;
    float: left;
    margin-left: 0;
}
.pagination ul li:nth-last-child(2){
    margin-right: 0;
    font-size: 12px;
    color: #333333 !important;
    text-decoration: none !important;
}
.pagination ul li:last-Child a{
    pointer-events: none;
    font-size: 12px;
    color: #333333 !important;
    text-decoration: none !important;
    cursor: pointer;
}
.pagination ul li:nth-last-child(2) a{
    pointer-events: none;
    font-size: 12px;
    color: #333333 !important;
    text-decoration: none !important;
    cursor: pointer;
}
.pagination ul li:last-Child a:hover{
    color: #666666;
    text-decoration: none;
}
.pagination ul li:nth-last-child(2) a:hover{
    color: #666666;
    text-decoration: none;
}
.pagination ul .jump{
    margin-top: -4px;
}
.pagination ul li a{
    font-size: 12px;
    color: #333333;
    text-decoration: underline;
    cursor: pointer;
}
.pagination ul li a:hover{
    font-size: 12px;
    color: #FF9900 !important;
    text-decoration: none;
}
.pagination ul li a:active{
    font-size: 12px;
    color: #ff6600 !important;
    text-decoration: none;
}
.newactive a{
    font-size: 12px;
    color: #ff7d00 !important;
    text-decoration: none !important;
}
.myPagination{
    width: 96%;
    height: 40px;
    margin: 0;
    padding: 0;
    position: absolute;
    bottom: 50px;
    position: fixed;
    font-size: 12px !important;
}
.pagination {
    width: 96%;
    height: 50px;
    margin: 0;
    padding: 0;
    background: #FFFFFF;
    padding-top: 16px;
    position: absolute;
    bottom: 0;
    left: 2%;
    position: fixed;
}
.tips{
    color: #ff0000;
}
@media (max-width: 760px) {
    .paddingLeft,.paddingRight{padding: 0}
}