package com.wanma.dubbox.model;
import java.sql.Timestamp;

import com.wanma.dubbox.model.common.BasicModel;

public class TblPublishEp extends BasicModel {
	private static final long serialVersionUID = 2285339576428143120L;
	private String img; //图片路径
	private String adr; //所在地址
	private String lgt; //经度
	private String ltt; //纬度
	private String prmNt; //参数说明
	private String nt; //备注
	private Integer sts; //状态 0未通过审核1通过审核（暂时没用）
	public String getImg() {
		return img;
	}

	public void setImg(String img) {
		this.img = img;
	}

	public String getAdr() {
		return adr;
	}

	public void setAdr(String adr) {
		this.adr = adr;
	}

	public String getLgt() {
		return lgt;
