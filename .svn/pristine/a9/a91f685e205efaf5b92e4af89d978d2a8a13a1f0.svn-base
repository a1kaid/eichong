		return ltt;
	}

	public void setLtt(String ltt) {
		this.ltt = ltt;
	}

	public String getPrmNt() {
		return prmNt;
	}

	public void setPrmNt(String prmNt) {
		this.prmNt = prmNt;
	}

	public String getNt() {
		return nt;
	}

	public void setNt(String nt) {
		this.nt = nt;
