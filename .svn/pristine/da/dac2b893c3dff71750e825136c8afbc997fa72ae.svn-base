@charset "utf-8";
* {
    padding: 0;
    margin: 0;
}

.col-sm-1, .col-sm-2, .col-sm-3, .col-sm-4, .col-sm-5, .col-sm-6, .col-sm-7, .col-sm-8, .col-sm-9, .col-sm-10, .col-sm-11, .col-sm-12 {
    padding: 0;
}

body {
    background: #F3F3F4;
    font-family: "微软雅黑";
    font-size: 12px;
}

.container {
    width: 98%;
    padding: 24px 24px 20px 20px;
}

.formBlock {
    width: 100%;
    min-height: 98px;
    height: auto !important;
    /*overflow: hidden;*/
    background: #FFFFFF;
    border-top: 3px solid #E5E5E5;
    padding: 26px 18px 20px 20px;
}

.clearfix {
    　　zoom: 1;
}

.clearfix:after {
    content: "";
    display: block;
    clear: both;
}

.btnBlock {
    width: 100%;
    /*min-height: 54px;*/
    padding: 10px 0 10px 20px;
    background: #F3F3F4;
    /*overflow: hidden;*/
    position: relative;
    margin-top: 4px;
}

.btnBlock .btnStyle {
    background: #ff7d00;
    color: #ffffff;
    border-radius: 4px;
    text-align: center;
    height: 28px;
    line-height: 28px;
}

.btnBlock .btnStyle:hover {
    cursor: pointer;
    background: #FF7D00;
    color: #FFFFFF;
}
table thead th {
    text-align: center;
    font-size: 14px;
    font-family: "微软雅黑";
}
table tbody tr td:nth-child(2) span{
    color: #337ab7 !important;
}
table tbody tr td:nth-child(2) span:hover {
    color: #FF7D00 !important;
    text-decoration: underline;
    cursor: pointer;
}

.userAccount {
    /*border: 1px solid #DCDCDC;*/
    height: 32px;
}

.userAccount .userAccountInput {
    display: inline;
    width: 100%;
    height: 100%;
    border: 1px solid #DCDCDC;
    outline: none;
    color: #666666;
}

.userAccount .userAccountInput:focus {
    display: inline;
    width: 100%;
    height: 100%;
    border: 1px solid #ff7d00;
    color: #666666;
}

.queryParam {
    line-height: 32px;
    font-size: 14px;
    text-align: center;
}

.queryBtn {
    width: 76px;
    height: 26px;
    text-align: center;
    line-height: 26px;
    border: 1px solid #DCDCDC;
    border-radius: 4px;
    color: #666666;
    margin: 10px auto;
    margin-top: 0;
}

.queryBtn:hover {
    width: 76px;
    height: 26px;
    text-align: center;
    line-height: 26px;
    background: #FF7D00;
    border: 1px solid #FF7D00;
    border-radius: 4px;
    color: #FFFFFF;
    cursor: pointer;
}

.showSet {
    max-width: 100%;
    min-height: 138px;
    height: auto !important;
    overflow: hidden;
    background: #FFFFFF;
    border-top: 4px solid #E7EAEC;
    padding: 26px 18px 20px 20px;
    margin-top: 30px;
    overflow-x: scroll;
}

table thead th {
    text-align: center;
    font-size: 14px;
    font-family: "微软雅黑";
}

table tbody tr td:nth-child(2) a:hover {
    color: #FF7D00 !important;
    text-decoration: underline;
    cursor: pointer;
}

.disabledBtn {
    width: 104px;
    height: 26px;
    display: block;
    margin: 0 auto;
    color: #000000;
    background: #fce6c4;
    text-align: center;
    line-height: 26px;
    border-radius: 4px;
}

.disabledBtn:hover {
    cursor: pointer;
}

.disabledContent {
    text-align: center;
    padding: 14px;
    line-height: 20px;
}

/*分页部分开始*/
.pagination {
    width: 100%;
    height: 50px;
    margin: 0;
    padding: 0;
    background: #FFFFFF;
    padding-top: 16px;
}
.pagination ul{
    list-style: none;
}
.pagination ul li {
    float: left;
    margin-right: 10px;
}

.pagination ul li:last-Child {
    list-style: none;
    float: left;
    margin-left: 0;
}

.pagination ul li:nth-last-child(2) {
    margin-right: 0;
    font-size: 12px;
    color: #333333 !important;
    text-decoration: none !important;
}

.pagination ul li:last-Child a {
    pointer-events: none;
    font-size: 12px;
    color: #333333 !important;
    text-decoration: none !important;
    cursor: pointer;
}

.pagination ul li:nth-last-child(2) a {
    pointer-events: none;
    font-size: 12px;
    color: #333333 !important;
    text-decoration: none !important;
    cursor: pointer;
}

.pagination ul li:last-Child a:hover {
    color: #666666;
    text-decoration: none;
}

.pagination ul li:nth-last-child(2) a:hover {
    color: #666666;
    text-decoration: none;
}

.pagination ul .jump {
    margin-top: -4px;
}

.pagination ul li a {
    font-size: 12px;
    color: #333333;
    text-decoration: underline;
    cursor: pointer;
}

.pagination ul li a:hover {
    font-size: 12px;
    color: #FF9900 !important;
    text-decoration: none;
}

.pagination ul li a:active {
    font-size: 12px;
    color: #ff6600 !important;
    text-decoration: none;
}

.newactive a {
    font-size: 12px;
    color: #ff7d00 !important;
    text-decoration: none !important;
}

