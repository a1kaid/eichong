/*首页通栏幻灯*/
.header { width: 100%; max-width: 1920px; margin: 0 auto; position: relative; }
.header_topguangao { width: 120%; max-width: 1920px; margin: 0 auto; position: relative; }
.ggLoading, .ggLoading2 { }
.header { height: 650px; }
.gg { width: 100%; height: 650px; position: relative; z-index: 1; overflow: hidden; margin: 0 auto; background: #d3d3d3 repeat-x; }
.ggLoading { position: absolute; left: 40%; top: 200px; width: 325px; text-align: center; height: 56px; background-position: 0 -274px; background-repeat: no-repeat; line-height: 56px; color: #9c9c9c; }
.ggLoading2 { width: 330px; height: 56px; background-position: 213px -330px; background-repeat: no-repeat; }
.ggLoading em { font-weight: bold; }
.ggs { width: 200%; height: 650px; left: -50%; top: 0; position: absolute; }
.ggBox { width: 1920px; height: 650px; margin: 0 auto; }
    .ggBox a { display: block; width: 1920px; height: 650px; position: absolute; z-index: 1; opacity: 0.1; }
    .ggBox img { display: block; width: 1920px; height: 650px; }
.ggb { position: absolute; width: 100%; left: 0; bottom: 0; height: 30px; z-index: 4; }
.ggBtns { width: 200px; height: 30px; margin: 0 auto; text-align:center;  }
    .ggBtns a { float: left; display: block; width: 40px; height: 30px; text-align: center; color: #848380; font-size: 14px; line-height: 30px;  position: center;  outline: none; background-repeat: no-repeat; cursor: pointer; }
        .ggBtns a em { display: block; width: 40px; height: 30px; margin: 0 auto; overflow: hidden; }
        .ggBtns a:hover { color: #e7e7e7; }
        .ggBtns a:focus { outline: none; }
        .ggBtns a.ggOn { color: #e7e7e7; background-position: 0 0; }
    .ggBtns a { background-image: url(../images/banner/gg2.png); }
a.ggOn { background-image: url(../images/banner/gg.png); }
