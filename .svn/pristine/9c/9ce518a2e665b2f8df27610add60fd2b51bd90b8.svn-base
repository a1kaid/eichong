@charset "utf-8";
body,html{
    font-family: '微软雅黑';
    background-color: #F3F3F4;;
}
.userBaseInfo{
    background-color: #ffffff;
    border-top: 4px solid #E5E5E5;
    margin-top: 20px;
    padding:14px 14px 20px 14px;
    position: relative;
}
.clearfix{
    　　zoom:1;
}

.clearfix:after{
    content:"";
    display:block;
    clear:both;
}
.ibox-content {
    background-color: #F3F3F4 !important;
    color: inherit;
    /* padding: 15px 20px 20px; */
    border-color: #e7eaec;
    -webkit-border-image: none;
    -o-border-image: none;
    border-image: none;
}
.goBack{
    display: inline-block;
    width: 80px;
    height: 30px;
    line-height: 30px;
    color: #ffffff;
    border-radius: 4px;
    text-align: center;
    background-color: #bfbfbf;
    margin-right: 20px;
}
.cursor{
    cursor: pointer;
}
.saveBtn{
    display: inline-block;
    width: 80px;
    height: 30px;
    line-height: 30px;
    color: #ffffff;
    background-color: #ff7d00;
    text-align: center;
    border-radius:4px;
    font-size: 13px;
    margin-right: 20px;
}
.btnBlock{
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    margin-top: 5px;
}
.titleTip{
    font-size: 15px;
    font-weight: 900;
    line-height: 38px;
    padding-left: 14px;
}
.baseInfoLeft{
    border: 1px solid #E7EAEC;
    border-right: 0;
    list-style: none;
    padding: 0;
}
.baseInfoRight{
    border: 1px solid #E7EAEC;
    list-style: none;
    padding: 0;
}
.baseInfoLeft li,.baseInfoRight li{
    height: 38px;
    line-height: 38px;
    /*padding: 0 20px;*/
    border-bottom: 1px solid #E7EAEC;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.baseInfoLeft li:last-child,.baseInfoRight li:last-child{
    border-bottom: 0;
}
.baseInfoLeft li:last-child{
    border-right: 1px solid #E7EAEC;
}
.commonUl li input{
    text-align: right;
    border: none;
    outline: none;
}
.commonUl li input:focus{
    border: 1px solid #ff7d00;
}
/*下拉框样式===========*/

ul {
    list-style: none;
}
.model-select-box {
    float: left;
    text-indent: 5px;
    position: relative;
}
.downRow {
    width: 17%;
    height: 100%;
    background: url(../../../img/downR.png) no-repeat center;
    position: absolute;
    right: 0;
    top: 0;
    border-left: 1px solid #DCDCDC;
    cursor: pointer;
    z-index: 0;
}

.model-select-text {
    height: 100%;
    width: 100%;
    display: flex;
    align-items: center;
    cursor: pointer;
    color: #666666;
    -moz-user-select: none;
    -webkit-user-select: none;
    user-select: none;
    border-left: 1px solid #dcdcdc;
}

.model-select-option {
    display: none;
    position: absolute;
    background: #fff;
    width: 100%;
    left: -1px;
    border: 1px solid #aaa;
    z-index: 5000;
    max-height: 200px;
    overflow-y: auto;
    overflow-x: hidden;
    padding: 0;
}

.model-select-option li {
    height: 32px;
    line-height: 32px;
    color: #666666;
    cursor: pointer;
    padding-left: 15px !important;
}

.model-select-option li.seleced {
    background: #C0C0C0;
    color: #fff;
}
.model-select-option li:hover{
    background: #ff7d00;
    color: #fff;
}
table thead th{
    text-align: center;
    font-size: 14px;
    font-family: "微软雅黑";
    background-color: #ffffff !important;
}
table tbody tr td{
    text-align: center;
    font-size: 14px;
    font-family: "微软雅黑";
    background-color: #ffffff !important;
}
table tbody tr td select{
    width: 100%;
    height: auto;
}
.showBtn{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    padding-right: 0;
}
.tableTip{
    font-size: 15px;
    font-weight: 900;
    line-height: 38px;
    padding-left: 14px;
}
.rightBtn{
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
}
.rightBtn div{
    display: inline-block;
    width: 80px;
    height: 30px;
    line-height: 30px;
    color: #ffffff;
    border-radius: 4px;
    text-align: center;
    background-color: #bfbfbf;
}
.rightBtn .bindBtn{
    margin-right: 20px;
}
/*弹框设计=================*/
.layui-layer-title {
    padding: 0 80px 0 20px !important;
    height: 42px;
    line-height: 42px;
    border-bottom: 1px solid #eee;
    font-size: 14px;
    color: #333;
    overflow: hidden;
    background-color: #F8F8F8;
    border-radius: 2px 2px 0 0;
}
.layui-layer-btn .layui-layer-btn0 {
    border-color: #FF7D00;
    background-color: #FF7D00;
    color: #fff;
    right: 10px;
    position: absolute;
    bottom: 14px;
}
.bindPile{
    width: 100%;
    margin: 0 auto;
    position: relative;
    overflow: auto;
}
.bindPile .bindPileSelect{
    position: absolute;
    position: fixed;
    top: 42px;
    width:96%;
    min-height: 34px;
    padding: 10px 0;
    background: #ffffff;
    z-index: 19891099;
    margin: 0 auto;
    border-bottom: 1px solid #E1E1E1;
}
.bindPile .bindPileSelect input{
    width: 160px;
    height: 32px;
    outline: none;
    border: none;
    border: 1px solid #E1E1E1;
    /*border-radius: 4px;*/
}
/*下拉筛选框*/
.bindPile .bindPileSelect .model-select-box {
    height: 32px;
    width: 120px;
    display: inline-block;
    margin-right: 10px;
    line-height: 32px;
    border: 1px solid #E7EAEC;
    float: left;
    text-indent: 5px;
    position: relative;
}

.bindPile .bindPileSelect  .downRow {
    width: 32px;
    height: 31px;
    background: url(../../../img/downR.png) no-repeat center;
    position: absolute;
    right: 0;
    top: 0;
    border-left: 1px solid #DCDCDC;
    cursor: pointer;
    z-index: 0;
}

.bindPile .bindPileSelect  .model-select-text {
    height: 32px;
    cursor: pointer;
    color: #666666;
    -moz-user-select: none;
    -webkit-user-select: none;
    user-select: none;
    border-left: 0;
}

.bindPile .bindPileSelect  .model-select-option {
    display: none;
    position: absolute;
    background: #fff;
    width: 100%;
    left: -1px;
    border: 1px solid #aaa;
    z-index: 5000;
    max-height: 200px;
    overflow-y: auto;
    overflow-x: hidden;
}

.bindPile .bindPileSelect  .model-select-option li {
    height: 22px;
    line-height: 22px;
    color: #666666;
    cursor: pointer;
}

.bindPile .bindPileSelect  .model-select-option li.seleced {
    background: #ff7d00;
    color: #fff;
}
.bindPile .bindPileSelect  .model-select-option li:hover{
    background: #ff7d00;
    color: #fff;
}
/**/
.bindPile .bindPileSelect .selectBtn{
    display: inline-block;
    width: 50px;
    height: 26px;
    line-height: 26px;
    text-align: center;
    color: #FFFFFF;
    border: 1px solid #FF7D00;
    border-radius: 4px;
    background: #FF7D00;
    margin-left: 20px;
    font-size: 12px;
    cursor: pointer;
}
.bindPileBlock{
    padding-top: 10px;
    margin-top: 40px;
    /*background-color: #FF0000;*/
}
.bindPileBlock .bindPileTable{
    width: 100%;
}
.table > thead > tr > th {
    vertical-align: bottom;
    border-bottom: 1px solid #ddd;
    font-size: 12px;
}
.table > thead > tr > th, .table > tbody > tr > th, .table > tfoot > tr > th, .table > thead > tr > td, .table > tbody > tr > td, .table > tfoot > tr > td {
    font-size: 12px;
}
.bindPileTable tr:nth-child(odd) {
    background: #FFFFFF;
}
.bindPileTable tr:nth-child(even) {
    background: #f5f5f5;
}
.myPagination{
    width: 96%;
    height: 40px;
    margin: 0;
    padding: 0;
    position: absolute;
    bottom: 50px;
    position: fixed;
    font-size: 12px !important;
}
.pagination {
    width: 96%;
    height: 50px;
    margin: 0;
    padding: 0;
    background: #FFFFFF;
    padding-top: 16px;
    position: absolute;
    bottom: 0;
    left: 2%;
    position: fixed;
}
/*分页部分开始*/
.pagination ul li{
    float: left;
    margin-right: 10px;
}
.pagination ul li:last-Child{
    list-style: none;
    float: left;
    margin-left: 0;
}
.pagination ul li:nth-last-child(2){
    margin-right: 0;
    font-size: 12px;
    color: #333333 !important;
    text-decoration: none !important;
}
.pagination ul li:last-Child a{
    pointer-events: none;
    font-size: 12px;
    color: #333333 !important;
    text-decoration: none !important;
    cursor: pointer;
}
.pagination ul li:nth-last-child(2) a{
    pointer-events: none;
    font-size: 12px;
    color: #333333 !important;
    text-decoration: none !important;
    cursor: pointer;
}
.pagination ul li:last-Child a:hover{
    color: #666666;
    text-decoration: none;
}
.pagination ul li:nth-last-child(2) a:hover{
    color: #666666;
    text-decoration: none;
}
.pagination ul .jump{
    margin-top: -4px;
}
.pagination ul li a{
    font-size: 12px;
    color: #333333;
    text-decoration: underline;
    cursor: pointer;
}
.pagination ul li a:hover{
    font-size: 12px;
    color: #FF9900 !important;
    text-decoration: none;
}
.pagination ul li a:active{
    font-size: 12px;
    color: #ff6600 !important;
    text-decoration: none;
}
.newactive a{
    font-size: 12px;
    color: #ff7d00 !important;
    text-decoration: none !important;
}
.bindPileTable > thead > tr > th {
    width: 100px !important;
}
