		this.usInCarCompanyId = usInCarCompanyId;
	}

	public String getUsinCarinfoId() {
		return usinCarinfoId;
	}

	public void setUsinCarinfoId(String usinCarinfoId) {
		this.usinCarinfoId = usinCarinfoId;
	}

	public String getPkUserId() {
		return pkUserId;
