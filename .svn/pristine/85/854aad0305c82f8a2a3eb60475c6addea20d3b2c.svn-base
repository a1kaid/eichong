package com.wanma.model;

import java.io.Serializable;
/**
 * 
 * tbl_equipmentversion表
 * @author mew
 *
 */
public class TblEquipmentVersion implements Serializable {
		int pkEquipmentVersion;
		String evProductID;//产品ID：电桩/集中器ID
		String evProductType;//产品类型：1：电桩 2 集中器
		String evFirmwareNumber;//固件版本
		String evFirmwareVersion;//固件版本号
		String evHardwareNumber;//硬件编号
		String evHardwareVersion;//硬件版本号
		String evFileMd5;//升级文件的md5值
		public int getPk_EquipmentVersion() {
			return pkEquipmentVersion;
		}
		public void setPkEquipmentVersion(int pkEquipmentVersion) {
			this.pkEquipmentVersion = pkEquipmentVersion;
		}
		public String getEvProductID() {
			return evProductID;
		}
		public void setEvProductID(String evProductID) {
			this.evProductID = evProductID;
		}
		public String getEvProductType() {
			return evProductType;
		}
		public void setEvProductType(String evProductType) {
			this.evProductType = evProductType;
		}
		public String getEvFirmwareNumber() {
			return evFirmwareNumber;
		}
		public void setEvFirmwareNumber(String evFirmwareNumber) {
			this.evFirmwareNumber = evFirmwareNumber;
		}
		public String getEvFirmwareVersion() {
			return evFirmwareVersion;
		}
		public void setEvFirmwareVersion(String evFirmwareVersion) {
			this.evFirmwareVersion = evFirmwareVersion;
		}
		public String getEvHardwareNumber() {
			return evHardwareNumber;
		}
		public void setEvHardwareNumber(String evHardwareNumber) {
			this.evHardwareNumber = evHardwareNumber;
		}
		public String getEvHardwareVersion() {
			return evHardwareVersion;
		}
		public void setEvHardwareVersion(String evHardwareVersion) {
			this.evHardwareVersion = evHardwareVersion;
		}
		public String getEvFileMd5() {
			return evFileMd5;
		}
		public void setEvFileMd5(String evFileMd5) {
			this.evFileMd5 = evFileMd5;
		}
		@Override
		public String toString() {
			return "TblEquipmentversion [pk_EquipmentVersion="
					+ pkEquipmentVersion + ", evProductID=" + evProductID
					+ ", evProductType=" + evProductType
					+ ", evFirmwareNumber=" + evFirmwareNumber
					+ ", evFirmwareVersion=" + evFirmwareVersion
					+ ", evHardwareNumber=" + evHardwareNumber
					+ ", evHardwareVersion=" + evHardwareVersion
					+ ", evFileMd5=" + evFileMd5 + "]";
		}



}