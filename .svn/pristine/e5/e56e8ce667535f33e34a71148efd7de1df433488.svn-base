/*我的收藏部分==========================================================================================*/
.MyCollection{
	height: 3rem;
	margin-left: 2rem;
	color: #808080;
	line-height: 3rem;
	font-size: 1.4rem;
}
.collectionList{
	width: 100%;
	height: auto;
	border-top: 0.1rem solid #C0C0C0;
	border-bottom: 0.1rem solid #C0C0C0;
	background: #FFFFFF;
}
.collectionList li{
	width: 100%;
	height: 4rem;
	border-bottom: 0.1rem solid #CCCCCC;
	line-height: 4rem;
	padding-left: 2rem;
	list-style: none;
	position: relative;
	font-size: 1.4rem;
	color: #808080;
}
.collectionList li .moreBtn{
	width: 30%;
	height: 3rem;
	line-height: 1.6rem;
	padding: 0;
	margin-left: 30%;
	color: #808080;
	
}
.collectionList li .moreBtn:active,.collectionList li .moreBtn:focus{
	outline-style: none;
	border: 0.1rem solid #CCCCCC;
}
