*{
	margin: 0;
	padding: 0;
}
body,html{
	font-size: 62.5%;
	background: #EBECED;
	font-family: "微软雅黑";
}
.col-xs-2,.col-xs-8,.col-xs-4{
	padding: 0;
	height: 4rem;
}
/*第一块===================================================*/
header{
	width: 100%;
	height: 4.6rem;
	background-color: #FF7D00;
}
header .backPage a img{
	width: 3rem;
	height: 3rem;
	margin-top: 0.6rem;
}
header .searchBlock{
	width: 65%;
	height: 2.8rem;
	border: 0.1rem solid #FFFFFF;
	border-radius: 3rem;
	margin-top: 0.8rem;
}
header .searchBlock input{
	width: 90%;
	height: 1.8rem;
	border:0;
	margin-left: 1rem;
	margin-top: 0.2rem;
	outline-style: none;
	background: #FF7D00;
}
header .searchBtn a{
	display: block;
	width: 100%;
	height: 100%;
	text-align: center;
	line-height: 4.6rem;
	text-decoration: none;
	color: #FFFFFF;
	font-size: 1.6rem;
}

