/*扫一扫=========================*/
.dcontent{
	height: 5rem;
	text-align: center;
	line-height: 5rem;
}
.dcontent img{
	width: 60%;
	height: 70%;
	
}
.menuBtn a:link,
.menuBtn a:visited,
.menuBtn a:hover,
.menuBtn a:visited{
	text-decoration: none;
}
.mui-bar-tab .mui-tab-item .mui-icon ~ .mui-tab-label{
	font-size:1.6rem;
}
/*去掉高德地图的logo*/
.amap-logo {
    right: 0 !important;
    left: auto !important;
    display: none;
}
.amap-copyright {
    right: 70px !important;
    left: auto !important;
    display: none;
} 
/*地图图标=============================================*/
.zoomin{
	width: 4.6rem;
	height: 4.6rem;
	border-radius: 3rem;
	/*background: #FF0000;*/
	position: absolute;
	right: 1rem;
	bottom: 12rem;
	background: url(../img/btn_map_add.png) no-repeat center;
	background-size:100% 100% ;
}
.zoomout{
	width: 4.6rem;
	height: 4.6rem;
	border-radius: 3rem;
	/*background: #FF0000;*/
	position: absolute;
	right: 1rem;
	bottom: 6rem;
	background: url(../img/btn_map_narrow.png) no-repeat center;
	background-size:100% 100% ;
}
.geolocation{
	width: 4.6rem;
	height: 4.6rem;
	border-radius: 3rem;
	/*background: #FF0000;*/
	position: absolute;
	left: 1rem;
	bottom: 6rem;
	background: url(../img/bth_map_positioning.png) no-repeat center;
	background-size:100% 100% ;
}
