/*下拉框样式===========*/

ul {
    list-style: none;
}

p {
    margin-bottom: 20px;
}

.model-select-box {
    height: 32px;
    line-height: 32px;
    border: 1px solid #E7EAEC;
    float: left;
    text-indent: 5px;
    position: relative;
    margin:0 1%;
}

.downRow {
    width: 32px;
    height: 31px;
    background: url(../../img/downR.png) no-repeat center;
    position: absolute;
    right: 0;
    top: 0;
    border-bottom: 1px solid #DCDCDC;
    border-left: 1px solid #DCDCDC;
    cursor: pointer;
    z-index: 0;
    transform: rotate(0deg);
}
.downRowClick{
    width: 32px;
    height: 30px;
    background: url(../../img/downR.png) no-repeat center;
    position: absolute;
    right: 0;
    top: 0;
    border-right: 1px solid #DCDCDC;
    cursor: pointer;
    z-index: 0;
    transform: rotate(-180deg);
}

.model-select-text {
    height: 32px;
    cursor: pointer;
    color: #666666;
    -moz-user-select: none;
    -webkit-user-select: none;
    user-select: none;
}

.model-select-option {
    display: none;
    position: absolute;
    background: #fff;
    width: 100%;
    left: -1px;
    border: 1px solid #aaa;
    z-index: 5000;
    max-height: 200px;
    overflow-y: auto;
    overflow-x: hidden;
}

.model-select-option li {
    min-height: 22px;
    line-height: 22px;
    color: #666666;
    cursor: pointer;
}

.model-select-option li.seleced {
    background: #ff7d00;
    color: #fff;
}
/*结束下拉框*/
.checkList {
    list-style: none;
    min-height: 30px;
    position: absolute;
    left: 0;
    top: 30px;
    z-index: 10000;
    background: #FFFFFF;
    color: #000000;
    padding: 10px 20px 10px 40px;
}
@media only screen and (max-width:890px) {
    .checkList {
        list-style: none;
        min-height: 30px;
        position: absolute;
        left: 0;
        top: 30px;
        z-index: 10000;
        background: #FFFFFF;
        color: #000000;
        padding: 10px;
    }
    .checkList li {
        height: 24px;
        line-height: 24px;
        width: 100% !important;
        clear: left;
    }

}
.checkList li {
    height: 24px;
    line-height: 24px;
    width: 50%;
    float: left;
}

.checkList li input {
    opacity: 0;
    filter: alpha(opacity=0);
    width: 12px;
    height: 12px;
    cursor: pointer;
    position: relative;
    top: -3px;
}

.checkbox li label {
    height: 30px;
    line-height: 30px;
    float: right;
}

.checkboxActiveStyle {
    width: 12px;
    height: 12px;
    background: #ff9d00;
    float: left;
    margin-top: 5px;
    margin-right: 4px;
}

.checkboxStyle {
    width: 12px;
    height: 12px;
    background: #DCDCDC;
    float: left;
    margin-top: 5px;
    margin-right: 4px;
}
.showSet{
