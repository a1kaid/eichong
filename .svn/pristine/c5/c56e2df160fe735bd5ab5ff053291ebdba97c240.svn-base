package com.wanma.ims.common.domain;

import java.util.ArrayList;
import java.util.List;

import com.wanma.ims.common.domain.base.BasicModel;

/**
 * 资金账户
 * 
 * @author bingo
 * @date 2017-06-16
 */
public class FinAccountDO extends BasicModel {

	private static final long serialVersionUID = 4991806560162268453L;

	/** 主键 */
	private Long accountId;

	/** 资金账户号 */
	private String accountNO;

	/** 支付密码 */
	private String accountPwd;

	/** 充值余额 */
	private Double accountBalance;

	/** 赠送余额 */
	private Double accountPresent;

	/** 付费策略 */
	private Integer payRule;

	/** 结算方式 1.储值账户 2.信用账户 */
	private Integer tradeType;

	/** 预警金额 */
	private Double accountWarn;

	/** 状态 1.正常 2.冻结 3.删除 */
	private Integer accountStatus;

	/** 是否删除 0.否 1.是 */
	private Integer isDel;

	/** 公司ID */
	private Long cpyId;

	private String userAccount; // 用户帐号

	private String tradeTypeName; // 结算方式名称

	private String accountStatusName; // 状态名称

	private String allocationAmount;// 分配金额
	
	private Long userId; //用户ID

	private List<Long> accountIdList = new ArrayList<Long>(); // 资金帐号ID

	public Long getAccountId() {
		return accountId;
	}

	public void setAccountId(Long accountId) {
		this.accountId = accountId;
	}

	public String getAccountNO() {
		return accountNO;
	}

	public void setAccountNO(String accountNO) {
		this.accountNO = accountNO;
	}

	public String getAccountPwd() {
		return accountPwd;
	}

	public void setAccountPwd(String accountPwd) {
		this.accountPwd = accountPwd;
	}

	public Double getAccountBalance() {
		return accountBalance;
	}

	public void setAccountBalance(Double accountBalance) {
		this.accountBalance = accountBalance;
	}

	public Integer getTradeType() {
		return tradeType;
	}

	public void setTradeType(Integer tradeType) {
		this.tradeType = tradeType;
	}

	public Double getAccountWarn() {
		return accountWarn;
	}

	public void setAccountWarn(Double accountWarn) {
		this.accountWarn = accountWarn;
	}

	public Integer getAccountStatus() {
		return accountStatus;
	}

	public void setAccountStatus(Integer accountStatus) {
		this.accountStatus = accountStatus;
	}

	public Integer getIsDel() {
		return isDel;
	}

	public void setIsDel(Integer isDel) {
		this.isDel = isDel;
	}

	public String getUserAccount() {
		return userAccount;
	}

	public void setUserAccount(String userAccount) {
		this.userAccount = userAccount;
	}

	public String getTradeTypeName() {
		return tradeTypeName;
	}

	public void setTradeTypeName(String tradeTypeName) {
		this.tradeTypeName = tradeTypeName;
	}

	public String getAccountStatusName() {
		return accountStatusName;
	}

	public void setAccountStatusName(String accountStatusName) {
		this.accountStatusName = accountStatusName;
	}

	public String getAllocationAmount() {
		return allocationAmount;
	}

	public void setAllocationAmount(String allocationAmount) {
		this.allocationAmount = allocationAmount;
	}

	public List<Long> getAccountIdList() {
		return accountIdList;
	}

	public void setAccountIdList(List<Long> accountIdList) {
		this.accountIdList = accountIdList;
	}

	public Double getAccountPresent() {
		return accountPresent;
	}

	public void setAccountPresent(Double accountPresent) {
		this.accountPresent = accountPresent;
	}

	public Integer getPayRule() {
		return payRule;
	}

	public void setPayRule(Integer payRule) {
		this.payRule = payRule;
	}

	public Long getCpyId() {
		return cpyId;
	}

	public void setCpyId(Long cpyId) {
		this.cpyId = cpyId;
	}
	
	public Long getUserId() {
		return userId;
	}
	
	public void setUserId(Long userId) {
		this.userId = userId;
	}

}
