#container1 .box .AssetStatistics span {
	font-size: 18px;
	color: #333333;
}
#container1 .box .formList {
	width: 100%;
	padding: 20px;
	background: #FFFFFF;
	/*border-bottom: 1px solid #C0C0C0;*/
}

