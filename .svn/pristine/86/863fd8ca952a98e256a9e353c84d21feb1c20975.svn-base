#container1 {
	height: 100%;
}
:-moz-placeholder {
	color: #666666; opacity:1;
}

::-moz-placeholder {
	color: #666666;opacity:1;
}

input:-ms-input-placeholder{
	color: #666666;opacity:1;
}

input::-webkit-input-placeholder{
	color: #666666;opacity:1;
}
#container1 .box {
	width: 100%;
	min-height: 50px;
	margin: 0 auto;
	background: #FFFFFF;
	padding:0 30px;
	position: absolute;
	position: fixed;
	top:63px;
	z-index: 100;
}
