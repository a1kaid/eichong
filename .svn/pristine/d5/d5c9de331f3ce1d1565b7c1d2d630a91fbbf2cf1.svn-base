@charset "utf-8";
body{
    height:auto!important;
    color: #333;
    font-size: 12px;
    background-color: #f3f3f4;
    font-family: "Microsoft YaHei", "微软雅黑" !important;
}
.userBaseInfo{
    background-color: #ffffff;
    border-top: 4px solid #E5E5E5;
    margin-top: 20px;
    padding: 14px;
    background-color: #ffffff;
}
.queryParam {
    line-height: 25px;
    font-size: 14px;
    text-align: left;
    color: #333;
}
.formBlock {
    width: 100%;
    min-height: 138px;
    height: 100%;
    /*overflow: hidden;*/
    background: #FFFFFF;
    border-top: 3px solid #E5E5E5;
    padding: 26px 18px 20px 20px;
}
.userAccount{
    /*border: 1px solid #DCDCDC;*/
    height: 32px;
}
.userAccount .userAccountInput{
    display: inline;
    width: 100%;
    height: 100%;
    border: 1px solid #DCDCDC;
    outline: none;
    color: #666666;
}
.userAccount .userAccountInput:focus{
    display: inline;
    width: 100%;
    height: 100%;
    border: 1px solid #ff7d00;
    color: #666666;
}
.col-sm-3,.radio-inline{
    font-size: 14px;
    line-height: 40px;
}
.radio-inline input{
    margin-top:13px!important;
}
.explain .queryParam{
    font-size: 12px;
}
.finManage-title{
    font-size: 1.5em;
    font-weight: bold;
    margin-bottom: 15px;
}
.link-company{
    font-size: 13px;
    margin-left:5%;
    text-decoration: underline;
}
.add-company{
    font-size: 13px;
    margin-left:5%;
    text-decoration: underline;
}
#cancelBtn {
    width: 80px;
    height: 30px;
    cursor: pointer;
    line-height: 30px;
    color: #ffffff;
    display: inline-block;
    border-radius: 4px;
    text-align: center;
    margin: 3% 0 0 3%;
    background-color: #bfbfbf;
}
#submitBtn {
    display: inline-block;
    width: 80px;
    height: 30px;
    line-height: 30px;
    color: #ffffff;
    background-color: #ff7d00;
    text-align: center;
    border-radius: 4px;
    font-size: 13px;
    margin: 3% 0 0 15%;
    cursor: pointer;
}
#changeBtn{
    display: inline-block;
    width: 80px;
    height: 30px;
    line-height: 30px;
    color: #ffffff;
    background-color: #ff7d00;
    text-align: center;
    border-radius: 4px;
    font-size: 13px;
    margin: 3% 0 0 15%;
    cursor: pointer;
}
#searchBtn{
    width: 100px;
    height: 30px;
    line-height: 30px;
    color: #ffffff;
    background-color: #ff7d00;
    text-align: center;
    border-radius: 4px;
    font-size: 13px;
    cursor: pointer;
}
.serviceFee .userAccount{
    width: 12%;
}
.serviceFee .queryParam{
    line-height: 30px;
}
.electricFee .userAccount{
    width: 12%;
}
.log{
    margin-left: 15px;
}
.electric-company .userAccount,.electric-company .cityName{
    width: 230px;
    float: left;
}
.cityQueryBlock {
    position: absolute;
    display: none;
    top: 32px;
    width: 230px;
    min-height: 210px;
    z-index: 100;
}
.cityQueryBlock .searchUl {
    max-height: 180px;
    background-color: #FFFFFF;
    overflow-y: auto;
    list-style: none;
    padding: 0;
    margin: 0;
    border: 1px solid #c1c1c1;
    border-top: 0;
}
.cityQueryBlock .searchUl li {
    line-height: 30px;
    padding-left: 6px;
}
.cityQueryBlock .searchUl li:hover {
    background-color: #ff7d00;
    color: #FFFFFF;
    padding-left: 6px;
    cursor: pointer;
}

