package com.wanma.dubbox.model;

import java.util.Date;

import com.wanma.dubbox.model.common.BasicModel;

/**
 * 
 * tbl_ConfigParameter表
 * 
 * @author songjf
 * 
 */
public class TblConfigparameter extends BasicModel {
	private static final long serialVersionUID = -232110207663427409L;
	private String name; // 配置参数名称
	private String memo; // 备注
	private Integer type; // 1：桩体配置参数 2：商城配置参数 3：其他配置参数。等等‘’‘’‘’‘
	private Integer sts; // 状态(0启用，1禁用)

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getMemo() {
		return memo;
	}

	public void setMemo(String memo) {
		this.memo = memo;
	}

	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}

	public Integer getSts() {
		return sts;
	}

	public void setSts(Integer sts) {
		this.sts = sts;
	}

}