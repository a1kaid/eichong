#container1 {
	height: 100%;
}
#container1 .box {
	width: 100%;
	min-height: 50px;
	margin: 0 auto;
	background: #FFFFFF;
	padding:0 30px;
	position: absolute;
	position: fixed;
	top:63px;
	z-index: 100;
}
#container1 .box2 {
	width: 100%;
	min-height: 420px;
	background: #FFFFFF;
	padding: 0;
}

#container1 .box .btnGroup {
	width: 100%;
	height: 60px;
	font-size: 12px;
	background: #FFFFFF;
	padding-right: 50px;
	margin-top: 16px;
	border-bottom: 1px solid #E5E5E5;
	position: relative;
}
#container1 .box .btnGroup span {
	display: inline-block;
	width: 76px;
	height: 28px;
	background: #FF7D00;
	color: #FFFFFF;
	margin-top: 16px;
	border-radius: 4px;
	text-align: center;
	line-height: 28px;
	margin-left: 20px;
	cursor: pointer;
}
.myTable tr:nth-child(odd) {
	background: #FFFFFF;
}
.myTable tr:nth-child(even) {
	background: #f5f5f5;
}
.padding30 {
	padding: 30px;
	padding-left: 30px;
	padding-top: 0;
	background: #FFFFFF;
	overflow-x: auto;
}
.myTable {
	overflow: auto;
	width: 100%;
	margin-top: 94px !important;
}
.myTable tr th {
	/*min-width: 90px;*/
	text-align: left;
	min-height: 39px !important;
	line-height: 39px !important;
	color: #333333;
	font-size: 12px;
	background: #F5F5F5;
	/*padding-left: 10px;*/
}
.myTable tr .smallWidth{
	min-width: 40px;
	text-align: left;
	min-height: 39px !important;
	line-height: 39px !important;
	color: #333333;
	font-size: 12px;
	background: #F5F5F5;
	/*padding-left: 10px;*/
}
.myTable tr td {
	text-align: left;
	min-height:19px !important;
	/*line-height: 19px !important;*/
	padding: 10px 0;
	padding-left: 10px;
}
.myTable tbody tr{
	min-height: 19px !important;
	font-size: 12px;
	color: #666666;
}
/*分页部分开始*/
.pagination {
	width: 100%;
	height: 50px;
	margin: 0;
	padding: 0;
	background: #FFFFFF;
	padding-top:16px ;
	position: absolute;
	bottom: 0;
	left: 14.6667%;
	position: fixed;
}
.pagination ul li:last-Child{
	list-style: none;
	float: left;
	margin-left: 0;
	
}
.pagination ul li:last-Child a{
	text-decoration: none;
	pointer-events: none;
}
.pagination ul li:nth-last-child(2) a{
	text-decoration: none;
	pointer-events: none;
}
.pagination ul li:last-Child a:hover{
	color: #666666;
	text-decoration: none;
}
.pagination ul li:nth-last-child(2) a:hover{
	color: #666666;
	text-decoration: none;
}
.pagination ul .jump{
	margin-top: -4px;
}
.pagination ul li a{
	font-size: 12px;
	color: #333333;
	text-decoration: underline;
	cursor: pointer;
}
.pagination ul li a:visited,.pagination ul li a:hover,.pagination ul li a:focus,.pagination ul li a:active{
	font-size: 12px;
	color: #FF9900;
	text-decoration: none;
}
.textInput{
	font-size: 12px;
	display: inline-block;
	width: 30px;
	height: 30px;
}

/*弹框样式*/
.pileMakerAdd{
	padding: 30px;
	font-size: 12px;
}
.pileMakerAdd .line{
	height: 24px;
	margin-top: 20px;
}
.pileMakerAdd .line .marginLeft30{
	display: inline-block;
	height: 26px;
	margin-left: 30px;
	font-size: 12px;
	border: 1px solid #E5E5E5;
	border-radius: 4px;
}