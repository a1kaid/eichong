@charset "utf-8";
html,body{
    width: 100%;
    height: 100%;
    padding: 0;
    margin: 0;
    background-color: #F3F3F4;
    font-family: "Microsoft YaHei", "微软雅黑" !important;
}
.container {
    width: 98%;
    padding: 24px 24px 20px 20px;
}

.formBlock {
    width: 100%;
    min-height: 138px;
    height: auto !important;
    overflow: hidden;
    background: #FFFFFF;
    border-top: 3px solid #E5E5E5;
    padding: 26px 18px 20px 20px;
    position: relative;
}
.clearfix{
    　　zoom:1;
}

.clearfix:after{
    content:"";
    display:block;
    clear:both;
}
.showSet{
    max-width: 100%;
    min-height: 138px;
    height: auto !important;
    overflow: hidden;
    background: #FFFFFF;
    border-top: 4px solid #E7EAEC;
    padding: 26px 18px 20px 20px;
    overflow-x: auto;
}
.showSet .mainTab{
    width: 100%;
    height: 42px;
    background: #e9e9e9;
}
.showSet .mainTab span {
    display: block;
    float: left;
    width: 100px;
    height: auto;
    line-height: 42px;
    text-align: center;
    background: #cccccc;
    cursor: pointer;
    font-size: 12px;
    color: #484848;
}
.showSet .mainTab .active {
    display: inline-block;
    width: 100px;
    height: auto;
    line-height: 42px;
    text-align: center;
    background: #ff7e01;
    cursor: pointer;
    font-size: 12px;
    color: #FFFFFF;
}
.mainTabContent{
    width: 100%;
    min-height: 200px;
    background-color: #f0f0f0;
}
.cursor{
    cursor: pointer;
}