(function(){$('input[name="rememberMe"]').prop("checked",false);$('input[name="usinPhone"]').val("");$('input[name="usInPassword"]').val("");var a=UserService.isRememberPwd();if(a){$('input[name="rememberMe"]').prop("checked",true);$('input[name="usinPhone"]').val(a.uname||"");$('input[name="usInPassword"]').val(a.pwd||"")}})();$(document).ready(function(){$("#login-form").on("valid.form",function(b,a){UserService.login(a)});$("#check-img").click(function(){$(this).attr("src",config.IValidCode+"?r="+Math.random())});$('input[name="rememberMe"]').click(function(){if(!$(this).prop("checked")){Cookie.remove(Cookie.REMEMBER);$('input[name="usInPassword"]').val("")}})});