body {
	padding-top: 50px;
	font-family: "微软雅黑" !important;
	font-size: 14px;
	background: #FFFFFF;
}
@font-face {
	/*font-family:;*/
	src: url(../fonts/glyphicons-halflings-regular.svg);
}
.sub-header {
	padding-bottom: 10px;
	border-bottom: 1px solid #eee;
}
.toggleClass {
	width: 100%;
	min-height: 990px;
}
.col-sm-offset-2,.col-md-offset-2{
	margin-left: 0;
}
.navbar-brand {
    float: left;
    height: 50px;
    padding: 26px 15px;
    font-size: 18px;
    line-height: 20px;
}
