/*logo部分*/
.logoText{
	list-style: none;
	padding-top: 10px;
	margin-right: 50px;
}
.logoText li a{
	float: left;
	height: 18px;
	font-size: 16px;
	color: #666666;
}
.logoText li a img{
	display: inline-block;
	border: 0;
	margin-top:-12px;
}
