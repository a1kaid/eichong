package com.wanma.ims.common.domain.bo;



/**
* <p>Description: 用户首页中账单</p>
* @author bingo
* @date 2017-7-31
 */
public class UserAccountBO {

	/** 充值 */
    private Double chongzhi;
    
    /** 消费 */
    private Double xiaofei;
    
    /** 优惠 */
    private Double youhui;
    
    /** 冻结 */
    private Double dongjie;
    
    /** 余额 */
    private Double yue;

	public Double getChongzhi() {
		return chongzhi;
	}

	public void setChongzhi(Double chongzhi) {
		this.chongzhi = chongzhi;
	}

	public Double getXiaofei() {
		return xiaofei;
	}

	public void setXiaofei(Double xiaofei) {
		this.xiaofei = xiaofei;
	}

	public Double getYouhui() {
		return youhui;
	}

	public void setYouhui(Double youhui) {
		this.youhui = youhui;
	}

	public Double getDongjie() {
		return dongjie;
	}

	public void setDongjie(Double dongjie) {
		this.dongjie = dongjie;
	}

	public Double getYue() {
		return yue;
	}

	public void setYue(Double yue) {
		this.yue = yue;
	}
}
