<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>实时数据</title>
		<link rel="stylesheet" type="text/css" href="static/css/consumeStatistics/consumeStatistics.css"/>
	</head>
	<body>
		<div class="container-statistics">
			<section class="header">
				<div class="title">
					实时数据
				</div>
			</section>
			<section class="unitData">
				<div class="realtimeBlock">
					<div class="unitDataTitle">
						充值金额（元）
					</div>
					<div class="unitDataValue" id="payValue">
					</div>
				</div>
				<div class="realtimeBlock">
					<div class="unitDataTitle">
						充值次数
					</div>
					<div class="unitDataValue" id="payCount">
					</div>
				</div>
				<div class="realtimeBlock">
					<div class="unitDataTitle">
						充电金额（元）
					</div>
					<div class="unitDataValue" id="chargeValue">
					</div>
				</div>
				<div class="realtimeBlock">
					<div class="unitDataTitle">
						充电次数
					</div>
					<div class="unitDataValue" id="chargeCount">
					</div>
				</div>
			</section>
			<section class="main">
				<!--充电金额（实时）&充电次数 （两条折线图）-->
				<div class="mainTab">
					<span class="active">充电（实时）</span>
				</div>
				<div class="mainTabContent">
					<div class="mainTabBlock" style="width:1000px;height: 400px;margin: 0 auto;" id="realtimeData">
						
					</div>
				</div>
				<div class="mainTabContent">
					<div class="detaiData">
						<div class="detailDateTitle">
							详细数据
						</div>
						<table class="myTable" style="margin-top:0 ;">
							<thead>
								<tr class="active">
									<th>时间</th>
									<th style="padding-left: 12px;">充电金额</th>
									<th>充电次数</th>
									<th>充值金额</th>
									<th>充值次数</th>
								</tr>
							</thead>
							<tbody id="realTimeTbody">
								
							</tbody>
						</table>
					</div>
				</div>
				
			</section>
		</div>
	</body>
	<script src="static/js/consumeStatistics/realTimeDate.js" type="text/javascript" charset="utf-8"></script>
</html>

