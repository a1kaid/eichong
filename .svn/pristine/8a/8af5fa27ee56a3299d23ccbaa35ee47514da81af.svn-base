@charset "utf-8";
html,body{
    width: 100%;
    height: 100%;
    padding: 0;
    margin: 0;
    background-color: #F3F3F4;
    font-family: "Microsoft YaHei", "微软雅黑" !important;
}
.container {
    width: 98%;
    padding: 24px 24px 20px 20px;
}

.formBlock {
    width: 100%;
    min-height: 138px;
    height: auto !important;
    overflow: hidden;
    background: #FFFFFF;
    border-top: 3px solid #E5E5E5;
    padding: 26px 18px 20px 20px;
    position: relative;
}
.clearfix{
    　　zoom:1;
}

.clearfix:after{
    content:"";
    display:block;
    clear:both;
}
.map{
    width: 100%;
    height: 550px;
    background-color: #424b5b;

}
.formBlock .fastCheck{
    position: absolute;
    top: 50px;
    right: 40px;
    width: 230px;
    min-height: 300px;
    background-color: #cccccc;
    z-index: 100;
}
/*第一部分城市模糊查询*/
.cityQueryBlock{
    width: 230px;
    min-height: 210px;
    background-color: #404a59;
}
.cityQueryBlock .searchInput{
    width: 230px;
    height: 30px;
    display: flex;
    background-color: #FFFFFF;
}
.cityQueryBlock .searchInput .cityName{
    width: 200px;
    height: 30px;
    padding: 6px;
    border: 0;
    outline: none;
    border: 1px solid #c1c1c1;
}
.cityQueryBlock .searchInput .searchBtn{
    width: 30px;
    height: 30px;
    border: 1px solid #c1c1c1;
    border-left: 0;
    background: url("../../img/search.png") no-repeat center;
}
.cityQueryBlock .searchUl{
    max-height: 180px;
    background-color: #FFFFFF;
    overflow-y: auto;
    list-style: none;
    padding: 0;
    margin: 0;
    border: 1px solid #c1c1c1;
    border-top: 0;
    display: none;
}
.cityQueryBlock .searchUl li{
    line-height: 30px;
    padding-left: 6px;
}
.cityQueryBlock .searchUl li:hover{
    background-color: #ff7d00;
    color: #FFFFFF;
    padding-left: 6px;
    cursor: pointer;
}
/*第二部分城市查询数据结果*/
.countReportCity{
    width: 230px;
    min-height: 30px;
    list-style: none;
    padding: 0;
    margin: 0;
    color: #FFFFFF;
}
.countReportCity li{
    line-height: 46px;
    border-bottom: 1px dashed #c1c1c1;
}
.countReportCity li:first-child{
    font-size: 20px;
}
.countReportCity li:last-child{
    font-size: 12px;
    color: #ff7d00;
    border: none;
}
.entryMap{
    text-decoration: underline;
    display: none;
}
.cursor{
    cursor: pointer;
}
.mapBlock{
    padding: 20px;
}
.mapIndexBlock{
    width: 100%;
    height: 600px;
    background: #eeeeee url("../../img/map_index.png") no-repeat center;
    background-size: 70% 70%;
    display: flex;
    flex-direction: column;
    /*justify-content: center;*/
    align-items: center;
    justify-content: space-between;
}
.titleFlag{
    font-weight: 600;
    font-family: "Microsoft YaHei", "微软雅黑";
    font-size: 14px;
    color: #ff7d00;
    line-height: 70px;
}
.companyFlag{
    font-weight: 600;
    font-family: "Microsoft YaHei", "微软雅黑";
    font-size: 12px;
    color: #a6a6a6;
    line-height: 50px;
}