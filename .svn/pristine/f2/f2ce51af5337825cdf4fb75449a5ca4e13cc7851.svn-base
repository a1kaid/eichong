@charset "utf-8";
body,html{
    font-family: '微软雅黑';
    background-color: #F3F3F4;;
}
.userBaseInfo{
    background-color: #ffffff;
    border-top: 4px solid #E5E5E5;
    margin-top: 20px;
    padding:14px 14px  60px 14px;
    position: relative;

}
.clearfix{
    　　zoom:1;
}

.clearfix:after{
    content:"";
    display:block;
    clear:both;
}
.ibox-content {
    background-color: #F3F3F4 !important;
    color: inherit;
    /* padding: 15px 20px 20px; */
    border-color: #e7eaec;
    -webkit-border-image: none;
    -o-border-image: none;
    border-image: none;
}
.goBack{
    width: 80px;
    height: 30px;
    line-height: 30px;
    color: #ffffff;
    border-radius: 4px;
    text-align: center;
    background-color: #bfbfbf;
    position: absolute;
    right: 10px;
    bottom: 18px;
}
.cursor{
    cursor: pointer;
}
.saveBtn{
    display: inline-block;
    width: 80px;
    height: 30px;
    line-height: 30px;
    color: #ffffff;
    background-color: #ff7d00;
    text-align: center;
    border-radius:4px;
    font-size: 13px;
    position: absolute;
    right: 120px;
    bottom: 18px;
}
.titleTip{
    font-size: 15px;
    font-weight: 900;
    line-height: 38px;
    padding-left: 14px;
}
.baseInfoLeft{
    border: 1px solid #E7EAEC;
    border-right: 0;
    list-style: none;
    padding: 0;
}
.baseInfoRight{
    border: 1px solid #E7EAEC;
    list-style: none;
    padding: 0;
}
.baseInfoLeft li,.baseInfoRight li{
    height: 38px;
    line-height: 38px;
    /*padding: 0 20px;*/
    border-bottom: 1px solid #E7EAEC;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.baseInfoLeft li:last-child,.baseInfoRight li:last-child{
    border-bottom: 0;
}
.baseInfoLeft li:last-child{
    border-right: 1px solid #E7EAEC;
}
.commonUl li input{
    text-align: right;
    border: none;
    outline: none;
}
.commonUl li input:focus{
    border: 1px solid #ff7d00;
}
/*下拉框样式===========*/

ul {
    list-style: none;
}
.model-select-box {
    float: left;
    text-indent: 5px;
    position: relative;
}
.downRow {
    width: 17%;
    height: 100%;
    background: url(../../../img/downR.png) no-repeat center;
    position: absolute;
    right: 0;
    top: 0;
    border-left: 1px solid #DCDCDC;
    cursor: pointer;
    z-index: 0;
}

.model-select-text {
    height: 100%;
    width: 100%;
    display: flex;
    align-items: center;
    cursor: pointer;
    color: #666666;
    -moz-user-select: none;
    -webkit-user-select: none;
    user-select: none;
    border-left: 1px solid #dcdcdc;
}

.model-select-option {
    display: none;
    position: absolute;
    background: #fff;
    width: 100%;
    left: -1px;
    border: 1px solid #aaa;
    z-index: 5000;
    max-height: 200px;
    overflow-y: auto;
    overflow-x: hidden;
    padding: 0;
}

.model-select-option li {
    min-height: 32px;
    line-height: 32px;
    color: #666666;
    cursor: pointer;
    padding-left: 15px !important;
}

.model-select-option li.seleced {
    background: #C0C0C0;
    color: #fff;
}
.model-select-option li:hover{
    background: #ff7d00;
    color: #fff;
}
.picBox{
    width: 200px;
    height: 200px;
    position: relative;
    border: 1px solid #CCCCCC;
    display: inline-block;
    margin: 10px;
}
.picBox .pic{
    width: 100%;
    height: 100%;
}
.delPic{
    width: 20px;
    height: 20px;
    position: absolute;
    right: 0;
    top: 0;
    border:1px solid #bbb;
    cursor: pointer;
    opacity:0.8;
    filter: alpha(opacity=80);
    background: #eee;
    text-align: center;
    line-height: 20px;
    font-size: 16px;
}
.fileBtn{
    display: inline-block;
    height: 38px;
    line-height: 38px;
    color: #fff;
    white-space: nowrap;
    text-align: center;
    font-size: 14px;
    border: none;
    border-radius: 2px;
    cursor: pointer;
    opacity: 0.8;
    filter: alpha(opacity=80);
}


