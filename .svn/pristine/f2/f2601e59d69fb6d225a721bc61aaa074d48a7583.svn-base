                            <!--</div>-->
                        <!--</div>-->

                        <!--充电点历史信息-->
                        <div class="searchBlock searchCon" style="background:#fff;">
                            <div class="search">
                                <span class="offLineSpan">
                                    <i class="offLineState">充电点历史信息</i>
                                </span>
                                <div class="row piPhoto" style="margin:0;text-align:center;padding-bottom:28px;">
                                    <span style="position: relative;">
                                        <i class="thickLine"></i>
                                        <i style="font-size:12px;color:#666;font-weight:900;margin-left:10px;">充电点照片</i>
                                    </span>
                                </div>
                                <div class="row  swiperPhoto" style="margin:0 0 30px 0;padding:0 50px;position:relative;">
                                    <div class="swiper-container">
                                        <div class="swiper-wrapper">

                                        </div>
                                    </div>
                                    <!-- 如果需要导航按钮 -->
                                    <div class="swiper-button-prev" style="position:absolute;top:60px;"></div>
                                    <div class="swiper-button-next" style="position:absolute;top:60px;"></div>
                                </div>
                                <div class="row" style="margin:0;border:1px solid #c9c9c9;border-bottom:0;">
                                    <div class="row" style="margin:0;text-align:center;height:auto;border-bottom:1px solid #c9c9c9;padding:12px 20px;">
                                            <span style="position: relative;">
                                                <i class="thickLine"></i>
                                                <i style="font-size:12px;color:#666;font-weight:900;margin-left:10px;">历史数据</i>
                                            </span>
                                    </div>
                                    <ul class="orderUl" id="orderUl">
                                        <li class="row">
                                            <span class="left">昨日充电订单</span>
                                            <span class="right">
                                                <i class="yesterdayOrderCount"></i>
                                                <i>笔</i>
                                            </span>
                                        </li>
                                        <li class="row">
                                            <span class="left">昨日充电量</span>
                                            <span class="right">
                                                <i class="yesterdayChargeCount"></i>
                                                <i>kwh</i>
                                            </span>
                                        </li>
                                        <li class="row">
                                            <span class="left">上月充电订单</span>
                                            <span class="right">
                                                <i class="lastMonthOrderCount"></i>
                                                <i>笔</i>
                                            </span>
                                        </li>
                                        <li class="row">
                                            <span class="left">上月充电量</span>
                                            <span class="right">
                                                <i class="lastMonthChargeCount"></i>
                                                <i>kwh</i>
                                            </span>
                                        </li>
                                        <li class="row">
                                            <span class="left">历史充电订单</span>
                                            <span class="right">
                                                <i class="allCountOrderCount"></i>
                                                <i>笔</i>
                                            </span>
                                        </li>
                                        <li class="row">
                                            <span class="left">累计充电量</span>
                                            <span class="right">
                                                <i class="allCountChargeCount"></i>
                                                <i>kwh</i>
                                            </span>
                                        </li>
                                        <li class="row">
                                            <span class="left">电桩故障</span>
                                            <span class="right">
                                                <i class="epError"></i>
                                                <i>次</i>
                                            </span>
                                        </li>
                                        <li class="row">
                                            <span class="left">异常订单</span>
                                            <span class="right">
                                                <i class="orderError"></i>
                                                <i>笔</i>
                                            </span>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="emptyDiv"></div>
