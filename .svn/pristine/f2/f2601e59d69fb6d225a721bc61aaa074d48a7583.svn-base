    <title>实时信息</title>
</head>
<body class="gray-bg">
    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="container-fluid">
            <div class="row">
                <div class="btnLine">
                </div>
            </div>
            <div class="row box">
                <div class="col-sm-12 col-md-3 col-xs-12 boxLeft" style="padding:0 10px;">
                    <div class="ibox float-e-margins">
                        <div class="">
                            <div class="ibox-content">
                                <div class="searchStationList">
                                    <div class="title row powerStationName"></div>
                                    <ul class="stationHeadUl" id="stationHeadUl">
                                        <!--<li class="stationHeadLi row">-->
                                        <!--<span class="headNumCon col-sm-4 col-xs-4">枪口1</span>-->
                                        <!--<span class="chargingModeCon col-sm-4 col-xs-4">直流120kw</span>-->
                                        <!--<span class="headStatusCon col-sm-4 col-xs-4"><i class="circle"></i>空闲</span>-->
                                        <!--</li>-->
                                    </ul>
                                    <div class="stationHeadPage">
                                        <div id="stationDetailPage" class="pagination ">

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-12 col-md-9 col-xs-12 boxRight" style="padding:0 10px;">
                    <div class="ibox float-e-margins">
                        <div class="ibox-content">
                            <!--电桩在线状态-->
                            <div class="searchBlock" style="border-top:4px solid #c9c9c9;">
                                <div class="search">
                                    <span class="offLineSpan">
                                        <i class="offLineState">电桩在线状态</i>
                                        <i class="electricPile active" style="margin-left:20px;">电桩</i>
                                        <i style="display: inline-block;width:2px;height:15px;background:#666;position:absolute;top:2px;"></i>
                                        <i class="muzzle" style="margin-left:7px;">枪口</i>
                                    </span>
                                    <ul class="row chargeUl chargeUl1 switchState1" id="chargeUl" style="margin:0;border:1px solid #c9c9c9;">
                                        <li class="chargingState commonState col-xs-3 col-xs-12">
                                            <img src="../../../images/charge.png" alt=""/>充电中
                                            <i class="chargingCount"></i>
                                        </li>
                                        <li class="freeState commonState col-xs-3 col-xs-12">
                                            <img src="../../../images/free.png" alt=""/>空闲
                                            <i class="freeCount"></i>
                                        </li>
                                        <li class="errorState commonState col-xs-3 col-xs-12">
                                            <img src="../../../images/error.png" alt=""/>故障
                                            <i class="errorCount"></i>
                                        </li>
                                        <li class="disconnectState commonState col-xs-3 col-xs-12">
                                            <img src="../../../images/disconnect.png" alt=""/>离线
                                            <i class="disconnectCount"></i>
                                        </li>
                                    </ul>
                                    <ul class="row chargeUl chargeUl2 switchState2" id="chargeUl1" style="margin:0;border:1px solid #c9c9c9;">
                                        <li class="chargingState commonState col-xs-3 col-xs-12">
                                            <img src="../../../images/charge.png" alt=""/>充电中
                                            <i class="chargingCountTwo"></i>
                                        </li>
                                        <li class="freeState commonState col-xs-3 col-xs-12">
                                            <img src="../../../images/free.png" alt=""/>空闲
                                            <i class="freeCountTwo"></i>
                                        </li>
                                        <li class="errorState commonState col-xs-3 col-xs-12">
                                            <img src="../../../images/error.png" alt=""/>故障
                                            <i class="errorCountTwo"></i>
                                        </li>
                                        <li class="disconnectState commonState col-xs-3 col-xs-12">
                                            <img src="../../../images/disconnect.png" alt=""/>离线
                                            <i class="disconnectCountTwo"></i>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                            <div class="emptyDiv"></div>
                            <!--地锁状态-->
                            <div class="searchBlock searchCon">
                                <div class="search">
                                    <span class="offLineSpan">
                                        <i class="offLineState">地锁状态</i>
                                    </span>
                                    <ul class="row lockStateUl" id="lockStateUl" style="margin:0;border:1px solid #c9c9c9;">
                                        <li class="nomalState chargingState commonState col-xs-3 col-xs-12">
                                            <img src="../../../images/nomalLock.png" alt=""/>升锁
                                            <i class="nomalCount"></i>
                                        </li>
                                        <li class="usingState freeState commonState col-xs-3 col-xs-12">
                                            <img src="../../../images/usingLock.png" alt=""/>降锁
                                            <i class="usingCount"></i>
                                        </li>
                                        <li class="errorState commonState col-xs-3 col-xs-12">
                                            <img src="../../../images/errorLock.png" alt=""/>故障
                                            <i class="errorCount"></i>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="emptyDiv"></div>
                        <!--车位状态-->
                        <!--<div class="searchBlock searchCon">-->
