package com.wanma.ims.common.domain;

import com.wanma.ims.common.domain.base.BasicModel;


/**
 * 账户关系 
 * @author bingo
 * @date 2017-06-13
 */
public class FinAccountRelationDO extends BasicModel{

	private static final long serialVersionUID = 8310558559568944392L;

	/** 主键 */
	private Long pkId;
	
	/** 用户ID */
	private Long userId;
	
	/** 账单科目ID   */
	private Long billAccountId;
	
	/** 资金账户ID */
	private Long accountId;
	
	/** 排序 1-N 1.优先执行 N.最后执行 */
	private Integer priority;
	
	/** 是否删除 0.否 1.是 */
	private Integer isDel;
	
	private String billAccountName;  //账单科目名称
	
	private String accountNO;  //资金账户号
	
	private String userAccount;//用户帐号

	private Integer cpyNumber;//公司标识
	
	public Long getPkId() {
		return pkId;
	}

	public void setPkId(Long pkId) {
		this.pkId = pkId;
	}

	public Long getUserId() {
		return userId;
	}

	public void setUserId(Long userId) {
		this.userId = userId;
	}

	public Long getBillAccountId() {
		return billAccountId;
	}

	public void setBillAccountId(Long billAccountId) {
		this.billAccountId = billAccountId;
	}

	public Long getAccountId() {
		return accountId;
	}

	public void setAccountId(Long accountId) {
		this.accountId = accountId;
	}

	public Integer getPriority() {
		return priority;
	}

	public void setPriority(Integer priority) {
		this.priority = priority;
	}

	public Integer getIsDel() {
		return isDel;
	}

	public void setIsDel(Integer isDel) {
		this.isDel = isDel;
	}

	public String getBillAccountName() {
		return billAccountName;
	}

	public void setBillAccountName(String billAccountName) {
		this.billAccountName = billAccountName;
	}

	public String getAccountNO() {
		return accountNO;
	}

	public void setAccountNO(String accountNO) {
		this.accountNO = accountNO;
	}

	public String getUserAccount() {
		return userAccount;
	}

	public void setUserAccount(String userAccount) {
		this.userAccount = userAccount;
	}

    public Integer getCpyNumber() {
        return cpyNumber;
    }

    public void setCpyNumber(Integer cpyNumber) {
        this.cpyNumber = cpyNumber;
    }
}