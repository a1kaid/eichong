package com.wanma.dubbox.model;

import com.wanma.dubbox.model.common.BasicModel;

/**
 * FileName ArayacakPosition.java
 * 
 * Version 1.0
 * 
 * 区县bean类
 */
public class TblArea extends BasicModel {
	private static final long serialVersionUID = 7398585829782696583L;
	/** 区县名称 */
	private String name;
	/** 热门城市标志 */
	private String htFlg;
	private String delFlg;
	private String creUs;
	private String udtUs;


	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getHtFlg() {
		return htFlg;
	}

	public void setHtFlg(String htFlg) {
		this.htFlg = htFlg;
	}

	public String getDelFlg() {
		return delFlg;
	}

	public void setDelFlg(String delFlg) {
		this.delFlg = delFlg;
	}


	public String getCreUs() {
		return creUs;
	}

	public void setCreUs(String creUs) {
		this.creUs = creUs;
	}


	public String getUdtUs() {
		return udtUs;
	}

	public void setUdtUs(String udtUs) {
		this.udtUs = udtUs;
	}

}
