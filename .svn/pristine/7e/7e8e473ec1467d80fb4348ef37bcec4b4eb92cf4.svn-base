package com.wanma.model;



/**
 * app政企版用户信息
 * @author lyh
 *
 */
public class GovBusiUser {
	
	private int userId;//用户ID
	private String userName;//用户姓名
	private String userPhone; // 手机号码
	private String userImage; // 头像
	private String companyName;//企业名称

	private int paymentRule;//付费策略 1.扣大账户 2.扣自己 3.为小账户配资
	private int tradeType;//结算方式：结算方式 1.后付费（信用账户） 2.预付费（储值账户）
	private String accountBalance;//用户余额	
	
	private int chargingRecordNum;//充电记录--已完成数量
	private int chargingNum;//充电记录--未完成（正在充电）数量
	private String puHiMonetary;//消费金额
	private String chReFrozenAmt;//冻结金额
	
	public int getUserId() {
		return userId;
	}
	public void setUserId(int userId) {
		this.userId = userId;
	}
	
	public String getUserName() {
		return userName;
	}
	public void setUserName(String userName) {
		this.userName = userName;
	}
	public String getUserPhone() {
		return userPhone;
	}
	public void setUserPhone(String userPhone) {
		this.userPhone = userPhone;
	}
	public String getUserImage() {
		return userImage;
	}
	public void setUserImage(String userImage) {
		this.userImage = userImage;
	}
	public String getCompanyName() {
		return companyName;
	}
	public void setCompanyName(String companyName) {
		this.companyName = companyName;
	}
	public int getPaymentRule() {
		return paymentRule;
	}
	public void setPaymentRule(int paymentRule) {
		this.paymentRule = paymentRule;
	}
	public int getTradeType() {
		return tradeType;
	}
	public void setTradeType(int tradeType) {
		this.tradeType = tradeType;
	}
	public String getAccountBalance() {
		return accountBalance;
	}
	public void setAccountBalance(String accountBalance) {
		this.accountBalance = accountBalance;
	}
	public int getChargingRecordNum() {
		return chargingRecordNum;
	}
	public void setChargingRecordNum(int chargingRecordNum) {
		this.chargingRecordNum = chargingRecordNum;
	}
	public int getChargingNum() {
		return chargingNum;
	}
	public void setChargingNum(int chargingNum) {
		this.chargingNum = chargingNum;
	}
	public String getPuHiMonetary() {
		return puHiMonetary;
	}
	public void setPuHiMonetary(String puHiMonetary) {
		this.puHiMonetary = puHiMonetary;
	}
	public String getChReFrozenAmt() {
		return chReFrozenAmt;
	}
	public void setChReFrozenAmt(String chReFrozenAmt) {
		this.chReFrozenAmt = chReFrozenAmt;
	}
	@Override
	public String toString() {
		return "GovBusiUser [userId=" + userId + ", userName=" + userName
				+ ", userPhone=" + userPhone + ", userImage=" + userImage
				+ ", companyName=" + companyName + ", paymentRule="
				+ paymentRule + ", tradeType=" + tradeType
				+ ", accountBalance=" + accountBalance + ", chargingRecordNum="
				+ chargingRecordNum + ", chargingNum=" + chargingNum
				+ ", puHiMonetary=" + puHiMonetary + ", chReFrozenAmt="
				+ chReFrozenAmt + "]";
	}
	
	


	
	
}
