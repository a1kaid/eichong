<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace必须指向Dao接口 -->
<mapper namespace="com.wanma.app.dao.AppRescueMapper">

	<select id="getRescueList" resultType="HashMap">
		select id,com_name,com_phone
		 from tbl_rescue order by com_name
	</select>
</mapper> 