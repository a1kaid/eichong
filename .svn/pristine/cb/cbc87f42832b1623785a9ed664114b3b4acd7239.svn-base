@charset utf-8;
*{
	margin: 0;
	padding: 0;
}
html, body {
    font-family:"黑体";
    width: 100%;
    height: 100%;
    font-size: 62.5%;
}
*{   
    -webkit-touch-callout:none;  /*系统默认菜单被禁用*/   
    -webkit-user-select:none; /*webkit浏览器*/   
    -khtml-user-select:none; /*早期浏览器*/   
    -moz-user-select:none;/*火狐*/   
    -ms-user-select:none; /*IE10*/   
    user-select:none;   
}
/*第三部分*/
.status{
	margin:0 2.8rem;
	min-height: 4rem;
}
.status .statusMark{
	min-height: 3rem;
	line-height: 3rem;
	font-size: 1.6rem;
	color: #000;
	margin-top: 3rem;
}
