	<div class="occupied">
		
	</div>
	<!--当前余额===============================================================================================-->
	<section>
		<div class="balanceTip">
			<p class="currentPrice">当前余额</p>
			<p class="price"><span id="balance">0.00</span><span class="unit">(元)</span></p>
			<a class="btn btn-block" style="dispaly:block;width: 5rem; height: 3rem;line-height: 1.6rem; color: #FFFFFF;text-decoration: none;background: #FF7D00;margin: 0 auto; text-align: center;" onclick="toWalletCharge()">充值</a>
		</div>
	</section>
	<!--交易记录=================================================================================================-->
	<div class="tradingRecord">
		<p class="tradTitle">交易记录</p>
		<div class="container">
			<div class="rows">
				<div class="col-xs-6" >
					<select name="" class="typeChoice" id="typeChoice">
						<option value="">所有消费</option>
						<option value="1">充电消费</option>
						<option value="2">预约消费</option>
						<option value="3">购物消费</option>
						<option value="4">充值</option>
					</select>
				</div>
				<div class="col-xs-6 dataChoice ">
					<!--<div id='pickDateBtn' type="button" class="mui-btn mui-btn-block startTime">2014-8-14</div><br /><br />-->
					<!--<span id="to"></span>-->
					<!--<div id='pickDateBtn2' type="button" class="mui-btn mui-btn-block endTime">2014-8-16</div>-->
					<input value="" class="appDate" readonly="readonly" name="appDate" id="appDate" type="text">
					<input value="" class="appDate" readonly="readonly" name="appDate" id="appDate2" type="text">
				</div>
				
			</div>
			<div class="filterList" id="filterList">
				<!--<div class="col-xs-4 average" id="kinds">
					预约
