@charset "utf-8";
*{
    margin: 0;
    padding: 0;
}
body {
    background: #F3F3F4;
    font-family: "微软雅黑";
    font-size: 12px;
}
.baseInfoBtn{
    flex-direction: row;
    width: 68px;
    height: 24px;
    border-radius: 4px;
    background: #f19149;
    color: #FFFFFF;
    text-align: center;
    line-height: 14px;
    padding: 4px 10px;
    /*margin-left: 18px;*/
}
.baseInfoBtn:hover{
    cursor: pointer;
}
.borderRight{
    border-right:1px solid #E7EAEC ;
}
.tdBlock{
    padding: 14px 26px;
    display: flex;
    justify-content: space-between;
    flex-direction: column;
}
.tdBlock span{
    display: inline-block;
    min-width: 56px;
    text-align: center;
}
.container {
    width: 98%;
    padding: 2px 24px 20px 20px;
}
.btnLine{
    width: 100%;
    height: 40px;
    background: #F3F3F4;
    position: relative;
}
.grayBlock{
    min-height: 138px;
    height: auto !important;
    background: #F06E57;
    border-top: 3px solid #E5E5E5;
    padding: 26px 18px 20px 20px;
}
.clearfix{
    　　zoom:1;
}

.clearfix:after{
    content:"";
    display:block;
    clear:both;
}
.emptyDiv{
    width: 100%;
    height: 16px;
    background: #F3F3F4;
}
.userBaseInfo{
     padding:20px 20px 14px 28px;
     display: flex;
     flex-direction: row;
     border-top: 4px solid #E7EAEC;
 }
.companyInfo{
    padding:20px 20px 14px 28px;
    display: flex;
    flex-direction: row;
    border-top: 4px solid #E7EAEC;
}

.companyInfo .headTable{
    flex: 1;
    width:100%;
}
.companyInfo .headTable .headList{
    border: 1px solid #E7EAEC;
    list-style: none;
}

/*基本资料列表部分*/
.companyBaseInfoList{
    padding: 20px 20px 12px 20px;
    border-top: 4px solid #E7EAEC;
}
.companyBaseInfoList .baseInfoShow{
    display: flex;
    padding-bottom: 12px;
    padding-left: 4px;
    justify-content: space-between;
}
.companyBaseInfoList .baseInfoShow .baseInfoTitle{
    font-size: 14px;
    color: #000000;
    font-family: "微软雅黑";
}
.companyBaseInfoList .baseInfoShow .baseBtnInfoUl{
    list-style: none;
    display: flex;
    border: 0;
}
.companyBaseInfoList .baseInfoShow .baseBtnInfoUl li{
    flex-direction: row;
    min-width: 68px;
    height: 24px;
    border-radius: 4px;
    background: #f19149;
    color: #FFFFFF;
    text-align: center;
    line-height: 14px;
    padding: 4px 10px;
    /*margin-left: 18px;*/
}
.marginLeft{
    margin-left: 18px;
}
.companyBaseInfoList .baseInfoShow .baseBtnInfoUl li a{
    color:#FFF;
}
.companyBaseInfoList .baseInfoShow .baseInfoBtn:hover,
.companyBaseInfoList .baseInfoShow .baseInfoBtn:active,
.companyBaseInfoList .baseInfoShow .baseInfoBtn:focus{
    background-color: #f19149;
    cursor: pointer;
}
.companyBaseInfoList .baseInfoUl{
    list-style: none;
    border: 1px solid #E7EAEC;
}

.companyBaseInfoList .baseInfoUl li{
    border-bottom: 1px solid #E7EAEC;
    padding: 14px 18px 10px;
    display: flex;
    justify-content: space-between;
    color: #666666;
    font-size: 12px;
}
.companyBaseInfoList .baseInfoUl li:last-child{
    border: 0;
}
.tdBlockTop{
    padding: 14px;
    /*display: flex;*/
    text-align: center;
    /*flex-direction: column;*/
}
.borderBottom{
    border-bottom: 1px solid #E7EAEC;
}
.tdBlockTop span{
    /*flex:1;*/
    display: block;
    text-align: center;
    margin:0 auto;
}
.AllocationBtn{
    background:#f19149;
    color:#fff;
}
.tdTop{
    /*display: flex;*/
    /*flex-direction: column;*/
    justify-content: center;
    align-items: center;
    margin: 0 auto;
}
.baseAccount{
    flex-direction: row;
    height: 24px;
    border-radius: 4px;
    background: #f19149;
    color: #FFFFFF;
    text-align: center;
    line-height: 14px;
    padding: 4px 10px;
    margin:0 auto;
    cursor:pointer;
}
.accountBig{
    display:flex;
    height:24px;
    margin-top:15px;
}
.accountBox{
    flex-direction: column;
}
.toAssign input{
    width: 100%;
    height: 34px;
    line-height: 34px;
    margin:5px 0;
}
.allocationAmount input{
    width: 100%;
    height: 34px;
    line-height: 34px;
    margin:5px 0;
}
.warningLayer input{
    width: 100%;
    height: 34px;
    line-height: 34px;
    margin:5px 0;
}
.layui-layer-title{
    text-align: center;
}
.accountTip{
    line-height:20px;
}
.companyInfoFont{
    font-weight: 900;
    color:#333;
}
.chargeName{
    display: flex;
    padding:15px 18px;
    justify-content: space-between;
}
.baseBtnInfoUl a:hover{
    color:#fff;
}
.errorStyle{
    color: #FF0000;
    height: 16px;
    line-height: 16px;
    margin-bottom:4px;
    font-size: 12px;
}
.changeCompanyNewName input{
    width: 100%;
    height: 34px;
    line-height: 34px;
    margin: 5px 0;
}
.companyInfoTab tr td{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.companyInfoTab tr{
    display: flex;
    flex-direction: row;
}
.marginRight{
    margin-right: 20px;
}
.cursorSpan{
    cursor: pointer;
    text-decoration: underline ;
}
.keytBtn{
    cursor: pointer;
}