@charset "utf-8";
* {
    padding: 0;
    margin: 0;
}
.col-sm-1,.col-sm-2,.col-sm-3,.col-sm-4,.col-sm-5,.col-sm-6,.col-sm-7,.col-sm-8,.col-sm-9,.col-sm-10,.col-sm-11,.col-sm-12{
    padding: 0;
}
body {
    background: #F3F3F4;
    font-family: "微软雅黑";
    font-size: 12px;
}

.container {
    width: 98%;
    padding: 24px 24px 20px 20px;
}

.formBlock {
    width: 100%;
    min-height: 98px;
    height: auto !important;
    /*overflow: hidden;*/
    background: #FFFFFF;
    border-top: 3px solid #E5E5E5;
    padding: 26px 18px 20px 20px;
}
.clearfix{
    　　zoom:1;
}

.clearfix:after{
    content:"";
    display:block;
    clear:both;
}
.showSet{
    max-width: 100%;
    min-height: 138px;
    height: auto !important;
    overflow: hidden;
    background: #FFFFFF;
    border-top: 4px solid #E7EAEC;
    padding: 26px 18px 20px 20px;
    margin-top: 24px;
    overflow-x: scroll;
}
.showSet .mainTab{
    width: 100%;
    height: 42px;
    background: #e9e9e9;
}
.showSet .mainTab span {
    display: block;
    float: left;
    width: 100px;
    height: auto;
    line-height: 42px;
    text-align: center;
    background: #cccccc;
    cursor: pointer;
    font-size: 12px;
    color: #484848;
}
.showSet .mainTab .active {
    display: inline-block;
    width: 100px;
    height: auto;
    line-height: 42px;
    text-align: center;
    background: #ff7e01;
    cursor: pointer;
    font-size: 12px;
    color: #FFFFFF;
}
.mainTabContent{
    width: 100%;
    min-height: 200px;
}
.mainTabContent .mainTabBlock{
    width:100%;
    height: 350px;
    margin: 0 auto;
}
.mainTabContent .detaiData .detailDateTitle {
    width: 100%;
    font-size: 14px;
    color: #484848;
    padding: 0 20px 18px 18px;
    background: url(../../../img/circle.png) no-repeat 0 4px;
}
.btnBlock{
    width: 100%;
    min-height: 54px;
    background: #FFFFFF;
    /*background: #F3F3F4;*/
    overflow: hidden;
    position: relative;
    margin-top: 24px;
}
.btnBlock .perBlock{
    height: 100px;
    display: flex;
    padding: 10px;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    line-height: 30px;
    border-right: 1px solid #F3F3F4;
}
.btnBlock .perBlock .tipTitle{
    font-size: 14px;
    color: #484848;
}
.btnBlock .perBlock  .tipValue{
    font-size: 30px;
    color: #484848;
}
.btnBlock .perBlock:last-child{
    border: 0;
}
.userAccount{
    /*border: 1px solid #DCDCDC;*/
    height: 32px;
}
.userAccount .spanBtn{
    display: inline-block;
    width:50%;
    border:1px solid #ccc;
    background-color: #FFFFFF;
    color:#333;
    margin:-1px;
    height:32px;
    line-height: 32px;
    text-align: center;
    cursor: pointer;
}
.userAccount .activeBtn{
    display: inline-block;
    background-color: #ff7d00;
    color:#ffffff;
}
.userAccount .userAccountInput{
    display: inline;
    width: 100%;
    height: 100%;
    border: 1px solid #DCDCDC;
    outline: none;
    color: #666666;
}
.userAccount .userAccountInput:focus{
    display: inline;
    width: 100%;
    height: 100%;
    border: 1px solid #ff7d00;
    color: #666666;
}
.queryParam{
    line-height: 32px;
    font-size: 14px;
    text-align: center;
}
.queryBtn{
    width: 76px;
    height: 26px;
    text-align: center;
    line-height: 26px;
    border: 1px solid #DCDCDC;
    border-radius: 4px;
    color: #666666;
    margin: 10px auto;
    margin-top: 0;
    margin-right: 0;
}
.queryBtn:hover{
    width: 76px;
    height: 26px;
    text-align: center;
    line-height: 26px;
    background: #FF7D00;
    border: 1px solid #FF7D00;
    border-radius: 4px;
    color: #FFFFFF;
    cursor: pointer;
}
/*下拉框样式===========*/

ul {
    list-style: none;
}

p {
    margin-bottom: 20px;
}

.model-select-box {
    height: 32px;
    line-height: 32px;
    border: 1px solid #E7EAEC;
    float: left;
    text-indent: 5px;
    position: relative;
}

.downRow {
    width: 32px;
    height: 31px;
    background: url(../../../img/downR.png) no-repeat center;
    position: absolute;
    right: 0;
    top: 0;
    border-left: 1px solid #DCDCDC;
    cursor: pointer;
    z-index: 0;
}

.model-select-text {
    height: 32px;
    cursor: pointer;
    color: #666666;
    -moz-user-select: none;
    -webkit-user-select: none;
    user-select: none;
}

.model-select-option {
    display: none;
    position: absolute;
    background: #fff;
    width: 100%;
    left: -1px;
    border: 1px solid #aaa;
    z-index: 5000;
    max-height: 200px;
    overflow-y: auto;
    overflow-x: hidden;
}

.model-select-option li {
    height: 22px;
    line-height: 22px;
    color: #666666;
    cursor: pointer;
}

.model-select-option li.seleced {
    background: #ff7d00;
    color: #fff;
}
table thead th{
    text-align: center;
    font-size: 14px;
    font-family: "微软雅黑";
}

table tbody tr td:nth-child(2) a:hover{
    color: #FF7D00 !important;
    text-decoration: underline;
    cursor: pointer;
}
table tbody tr td:nth-child(3) a:hover{
    color: #FF7D00 !important;
    text-decoration: underline;
    cursor: pointer;
}
/*分页部分开始*/
.pagination {
    width: 100%;
    height: 50px;
    margin: 0;
    padding: 0;
    background: #FFFFFF;
    padding-top:16px ;
}
.pagination ul li{
    float: left;
    margin-right: 10px;
}
.pagination ul li:last-Child{
    list-style: none;
    float: left;
    margin-left: 0;
}
.pagination ul li:nth-last-child(2){
    margin-right: 0;
    font-size: 12px;
    color: #333333 !important;
    text-decoration: none !important;
}
.pagination ul li:last-Child a{
    pointer-events: none;
    font-size: 12px;
    color: #333333 !important;
    text-decoration: none !important;
    cursor: pointer;
}
.pagination ul li:nth-last-child(2) a{
    pointer-events: none;
    font-size: 12px;
    color: #333333 !important;
    text-decoration: none !important;
    cursor: pointer;
}
.pagination ul li:last-Child a:hover{
    color: #666666;
    text-decoration: none;
}
.pagination ul li:nth-last-child(2) a:hover{
    color: #666666;
    text-decoration: none;
}
.pagination ul .jump{
    margin-top: -4px;
}
.pagination ul li a{
    font-size: 12px;
    color: #333333;
    text-decoration: underline;
    cursor: pointer;
}
.pagination ul li a:hover{
    font-size: 12px;
    color: #FF9900 !important;
    text-decoration: none;
}
.pagination ul li a:active{
    font-size: 12px;
    color: #ff6600 !important;
    text-decoration: none;
}
.newactive a{
    font-size: 12px;
    color: #ff7d00 !important;
    text-decoration: none !important;
}