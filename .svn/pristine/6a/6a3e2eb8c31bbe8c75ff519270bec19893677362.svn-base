@charset "utf-8";
*{
	margin: 0;
	padding: 0;
}
body {
	background: #F3F3F4;
	font-family: "微软雅黑";
	font-size: 12px;
}
.orange{
	color: #FF7D00;
}
.orange:hover{
	cursor: pointer;
}
.cpyNameStyle{
	color: #003333;
}
.openDisable{
	display: inline-block;
	background: #ff7d00;
	color: #FFFFFF;
	width: 42px;
	height: 25px;
	border-radius: 4px;
	line-height: 25px;
	margin-top: -4px;
	font-size:12px;
	text-align: center;
}
.openDisable:hover{
	cursor: pointer;
	background:#c76303;
}
.leftBorder{
	display: inline-block;
	border-left: 1px solid #C0C4CD;
	margin-left: 10px;
	padding-left: 8px;
	color: #FF7D00;
}
.leftBorder:hover{
    cursor: pointer;
	text-decoration:underline;
}
.borderRight{
	border-right:1px solid #E7EAEC ;
}
.tdBlockTop{
    padding: 14px;
    display: flex;
    text-align: center;
    flex-direction: column;
}
.tdBlock span{
    display: block;

}
.tdBlock{
	padding: 14px;
	display: flex;
	justify-content: space-between;
}
.tdBlock span{
	display: inline-block;
}
.container {
	width: 98%;
	padding: 2px 24px 20px 20px;
}
.btnLine{
	width: 100%;
	height: 20px;
	background: #F3F3F4;
	position: relative;
}
.btnLine .goBack:hover{
	background: #FF7D00;
	color: #FFFFFF;
	border: 1px solid #ff7d00;
	cursor: pointer;
}
.btnLine .goBack{
	width: 68px;
	height: 24px;
	border: 1px solid #C0C0C0;
	color: #666666;
	text-align: center;
	line-height: 24px;
	border-radius: 4px;
	position: absolute;
	top: 6px;
	right: 14px;
}

.grayBlock{
	min-height: 138px;
	height: auto !important;
	background: #F06E57;
	border-top: 3px solid #E5E5E5;
	padding: 26px 18px 20px 20px;
}
.clearfix{
　　zoom:1;
}

.clearfix:after{
	content:"";
	display:block;
	clear:both;
}
.emptyDiv{
	width: 100%;
	height: 20px;
	background: #F3F3F4;
}
.userBaseInfo{
	padding:20px 20px 14px 28px;
	display: flex;
	flex-direction: row;
	border-top: 4px solid #c9c9c9;
}

.userBaseInfo .headImage{
	width: 94px;
	min-height: 94px;
	/*background: #0055AA;*/
	margin-right: 18px;
	display: flex;
	flex-direction: column;
	align-items: center;
	color: #666666;
}
.userBaseInfo .headImage img{
	display: block;
	width: 94px;
	height: 94px;
}
.userBaseInfo .headImage .editBtn{
	margin-top: 6px;
}
.userBaseInfo .headImage .editBtn:hover{
	color: #FF7D00;
	cursor: pointer;
}
.userBaseInfo .headTable{
	flex: 1;
	/*background: #00625A;*/
	/*padding-bottom: 20px;*/
}
.userBaseInfo .headTable .headList{
	border: 1px solid #E7EAEC;
	list-style: none;
}
.userBaseInfo .headTable .headList li{
	/*padding: 18px;*/
	font-size: 12px;
}.userBaseInfo .headTable .headList li:first-child{
	border-bottom: 1px solid #E7EAEC;
}
/*基本资料列表部分*/
.userBaseInfoList{
	padding: 20px;
	border-top: 4px solid #c9c9c9;
}
.userBaseInfoList .baseInfoShow{
	display: flex;
	padding-bottom: 12px;
	justify-content: space-between;
}
.userBaseInfoList .baseInfoShow .baseInfoTitle{
	font-size: 15px;
	color: #000000;
	font-family: "微软雅黑";
	font-weight:900;
	color:#666;
}
.userBaseInfoList .baseInfoShow .baseInfoBtn{
	min-width: 42px;
	height: 25px;
	border-radius: 4px;
	background: #ff7d00;
	color: #FFFFFF;
	text-align: center;
	line-height: 16px;
    padding: 4px 9px;
	font-size:12px;
}
.userBaseInfoList .baseInfoShow .baseInfoBtn a{
    display: block;
    width: 100%;
    height: 100%;
    color: #ffffff;
}
.userBaseInfoList .baseInfoShow .baseInfoBtn:hover,
.userBaseInfoList .baseInfoShow .baseInfoBtn:active,
.userBaseInfoList .baseInfoShow .baseInfoBtn:focus{
	background-color: #c76303;
	cursor: pointer;
}
.userBaseInfoList .baseInfoUl{
	list-style: none;
	border: 1px solid #DCDCDC;
}
.userBaseInfoList .editUserInfo li{
	padding: 0 !important;
}
.userBaseInfoList .editUserInfo li span{
	display: block;
	padding: 14px 18px 10px;
}
.userBaseInfoList .editUserInfo li input{
	text-align: right;
	border: 0;
	outline: none;
	padding-right: 15px;
	/*display: block;*/
}
/*下拉框样式===========*/

ul {
	list-style: none;
}
.model-select-box {
	float: left;
	text-indent: 5px;
	position: relative;
}
.downRow {
	width: 32px;
	height: 44px;
	background: url(../../img/downR.png) no-repeat center;
	position: absolute;
	right: 0;
	top: 0;
	border-bottom: 1px solid #DCDCDC;
	border-left: 1px solid #DCDCDC;
	cursor: pointer;
	z-index: 0;
	transform: rotate(0deg);
}
.downRowClick{
	width: 32px;
	height: 44px;
	background: url(../../img/downR.png) no-repeat center;
	position: absolute;
	right: 0;
	top: 0;
	border-right: 1px solid #DCDCDC;
	cursor: pointer;
	z-index: 0;
	transform: rotate(-180deg);
}

.model-select-text {
	height: 100%;
	width: 100%;
	display: flex;
	align-items: center;
	cursor: pointer;
	color: #666666;
	-moz-user-select: none;
	-webkit-user-select: none;
	user-select: none;
}

.model-select-option {
	display: none;
	position: absolute;
	background: #fff;
	width: 100%;
	left: -1px;
	border: 1px solid #aaa;
	z-index: 5000;
	max-height: 200px;
	overflow-y: auto;
	overflow-x: hidden;
}

.model-select-option li {
	min-height: 32px;
	line-height: 32px;
	color: #666666;
	cursor: pointer;
	padding-left: 15px !important;
}

.model-select-option li.seleced {
	background: #C0C0C0;
	color: #fff;
}
.userBaseInfoList .editUserInfo li input:focus{
	border: 1px solid #FF7D00;
}
.userBaseInfoList .editUserInfo li span:first-child{
	border-right: 1px solid #E7EAEC;
}
.userBaseInfoList .baseInfoUl li{
	border-bottom: 1px solid #DCDCDC;
	padding: 10px 20px 10px;
	display: flex;
	justify-content: space-between;
	color: #666666;
	font-size: 14px;
}
.userBaseInfoList .baseInfoUl li:last-child{
	border: 0;
}
.ucCardNumber{
    width: 100%;
    border: 1px solid #e1edf7;
    margin-top: 10px;
}
.ucCardNumber input{
    width: 100%;
    height: 28px;
    outline: none;
    border: 0;
    line-height: 28px;
}
.toLoadCardHome:hover{
    cursor: pointer;
    color: #ff7d00;
    text-decoration: underline;
}
/*编辑按钮样式*/
.file {
    position: relative;
    display: inline-block;
    border-radius: 4px;
    overflow: hidden;
    color: #1E88C7;
    text-decoration: none;
    text-indent: 0;
    line-height: 20px;
    border: 0;
    outline: none;
	float:right;
}
.file input {
    position: absolute;
    font-size: 100px;
    right: 0;
    top: 0;
    opacity: 0;
}
.file input:hover{
    cursor: pointer;
}
.file:hover {
    color: #ff7d00;
    outline: none;
    text-decoration: none;
}
.baseInfoUl li span{
	-ms-flex-pack:justify;
}
.userInfoTab tr td{
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
}
.userInfoTab tr{
	display: flex;
	flex-direction: row;
}
.deleteLink{
    color: #ff7d00;
    text-decoration: underline;
    cursor: pointer;
}
.cursor{
    cursor: pointer;
}
.layui-layer-btn .layui-layer-btn0 {
    border-color: #FF7D00 !important;
    background-color: #FF7D00 !important;
    color: #fff;
}

.layui-layer-btn a {
    height: 28px !important;
    line-height: 28px !important;
    margin: 0 6px !important;
    padding: 0 15px !important;
    border: 1px solid #dedede !important;
    background-color: #f1f1f1 !important;
    color: #333;
    border-radius: 2px !important;
    font-weight: 400!important;
    cursor: pointer !important;
    text-decoration: none!important;
}
.renderBtn{
    min-width: 20px;
    display: flex;
    flex-direction: row;
}
.renderBtn .accountRefund{
    margin-right: 20px;
}
.red{
    color: #FF0000;
}
.fontWeight{
    font-weight: 900;
    line-height: 30px;
}
.toPurchaseHistoryBtn{
    color:#337ab7
}
.toPurchaseHistoryBtn:hover{
    color: #ff7d00;
}
/*弹框设计=================*/
.bindPile{
	width: 100%;
	margin: 0 auto;
	position: relative;
	overflow: auto;
}
.bindPile .bindPileSelect{
	position: absolute;
	position: fixed;
	top: 42px;
	width:96%;
	min-height: 34px;
	padding: 10px 0;
	background: #ffffff;
	z-index: 19891099;
	margin: 0 auto;
	border-bottom: 1px solid #E1E1E1;
}
.bindPile .bindPileSelect input{
	width: 160px;
	height: 32px;
	outline: none;
	border: none;
	border: 1px solid #E1E1E1;
	text-indent: 5px;
	/*border-radius: 4px;*/
}
/**/
.bindPile .bindPileSelect .selectBtn{
	display: inline-block;
	width: 50px;
	height: 26px;
	line-height: 26px;
	text-align: center;
	color: #FFFFFF;
	border: 1px solid #FF7D00;
	border-radius: 4px;
	background: #FF7D00;
	margin-left: 20px;
	font-size: 12px;
	cursor: pointer;
}
.bindPileBlock{
	padding-top: 10px;
	margin-top: 40px;
	padding-bottom: 35px;
	/*background-color: #FF0000;*/
}
.bindPileBlock .bindPileTable{
	width: 100%;
}
.table > thead > tr > th {
	vertical-align: bottom;
	border-bottom: 1px solid #ddd;
	font-size: 12px;
}
.table > thead > tr > th, .table > tbody > tr > th, .table > tfoot > tr > th, .table > thead > tr > td, .table > tbody > tr > td, .table > tfoot > tr > td {
	font-size: 12px;
    text-align: center;
}
.bindPileTable tr:nth-child(odd) {
	background: #FFFFFF;
    text-align: center;
}
.bindPileTable tr:nth-child(even) {
	background: #f5f5f5;
    text-align: center;
}
.myPagination{
	width: 96%;
	height: 40px;
	margin: 0;
	padding: 0;
	position: absolute;
	bottom: 50px;
	position: fixed;
	font-size: 12px !important;
}
.pagination {
	width: 96%;
	height: 50px;
	margin: 0;
	padding: 0;
	background: #FFFFFF;
	padding-top: 16px;
	position: absolute;
	bottom: 0;
	left: 2%;
	position: fixed;
}
/*分页部分开始*/
.pagination ul li{
	float: left;
	margin-right: 10px;
}
.pagination ul li:last-Child{
	list-style: none;
	float: left;
	margin-left: 0;
}
.pagination ul li:nth-last-child(2){
	margin-right: 0;
	font-size: 12px;
	color: #333333 !important;
	text-decoration: none !important;
}
.pagination ul li:last-Child a{
	pointer-events: none;
	font-size: 12px;
	color: #333333 !important;
	text-decoration: none !important;
	cursor: pointer;
}
.pagination ul li:nth-last-child(2) a{
	pointer-events: none;
	font-size: 12px;
	color: #333333 !important;
	text-decoration: none !important;
	cursor: pointer;
}
.pagination ul li:last-Child a:hover{
	color: #666666;
	text-decoration: none;
}
.pagination ul li:nth-last-child(2) a:hover{
	color: #666666;
	text-decoration: none;
}
.pagination ul .jump{
	margin-top: -4px;
}
.pagination ul li a{
	font-size: 12px;
	color: #333333;
	text-decoration: underline;
	cursor: pointer;
}
.pagination ul li a:hover{
	font-size: 12px;
	color: #FF9900 !important;
	text-decoration: none;
}
.pagination ul li a:active{
	font-size: 12px;
	color: #ff6600 !important;
	text-decoration: none;
}
.newactive a{
	font-size: 12px;
	color: #ff7d00 !important;
	text-decoration: none !important;
}
.bindPileTable > thead > tr > th {
	width: 100px !important;
}
.sureBtn{
    height: 28px !important;
    padding: 0 15px !important;
    border-color: #FF7D00;
    background-color: #FF7D00;
    color: #fff;
    right: 20px;
    position: absolute;
    bottom: 14px;
    position: fixed;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
    line-height: 28px;
    text-align: center;
}
